<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema
        xmlns:bom="http://tibco.com/businessevents/releases/bom/5.1"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://tibco.com/businessevents/releases/bom/5.1"
        elementFormDefault="unqualified"
        attributeFormDefault="unqualified"
        >


    <xsd:element name="add-on" type="bom:add-on-type"/>
    <xsd:element name="add-ons" type="bom:add-ons-type"/>
    <xsd:element name="assemblies" type="bom:assemblies-type"/>
    <xsd:element name="assembly" type="bom:assembly-type"/>
    <xsd:element name="assembly-ref" type="bom:assembly-ref-type"/>
    <xsd:element name="bom" type="bom:bom-type"/>
    <xsd:element name="build" type="xsd:string"/>
    <xsd:element name="file-set" type="bom:file-set-type"/>
    <xsd:element name="file-set-ref" type="bom:file-set-ref-type"/>
    <xsd:element name="file-sets" type="bom:file-sets-type"/>
    <xsd:element name="installed-ga" type="xsd:string"/>
    <xsd:element name="installed-hf" type="xsd:string"/>
    <xsd:element name="source" type="xsd:string"/>
    <xsd:element name="version" type="xsd:string"/>


    <xsd:complexType name="add-on-type">
        <xsd:complexContent>
            <xsd:extension base="bom:component-type">
                <xsd:sequence>
                    <xsd:element ref="bom:assembly-ref" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="add-ons-type">
        <xsd:sequence>
            <xsd:element ref="bom:add-on" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="assemblies-type">
        <xsd:sequence>
            <xsd:element ref="bom:assembly" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="assembly-ref-type">
        <xsd:attribute name="ref" type="xsd:IDREF"/>
    </xsd:complexType>

    <xsd:complexType name="assembly-type">
        <xsd:complexContent>
            <xsd:extension base="bom:component-type">
                <xsd:sequence>
                    <xsd:element ref="bom:file-set-ref" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="port-specific"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="bom-type">
        <xsd:complexContent>
            <xsd:extension base="bom:component-type">
        <xsd:sequence>
            <xsd:element ref="bom:add-ons"/>
            <xsd:element ref="bom:assemblies"/>
            <xsd:element ref="bom:file-sets"/>
        </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="component-type">
        <xsd:attribute name="name" type="xsd:ID"/>
    </xsd:complexType>

    <xsd:complexType name="file-set-ref-type">
        <xsd:attribute name="ref" type="xsd:IDREF"/>
    </xsd:complexType>

    <xsd:complexType name="file-set-type">
        <xsd:complexContent>
            <xsd:extension base="bom:component-type">
                <xsd:sequence>
                    <xsd:element ref="bom:source" maxOccurs="unbounded"/>
                    <xsd:element ref="bom:build" minOccurs="1" maxOccurs="1"/>
                    <xsd:element ref="bom:assembly" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element ref="bom:installed-ga" maxOccurs="unbounded"/>
                    <xsd:element ref="bom:installed-hf" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="type" type="xsd:string"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="file-sets-type">
        <xsd:sequence>
            <xsd:element ref="bom:file-set" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>
