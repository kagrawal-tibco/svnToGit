Bundle-Description=BusinessEvents Data Modeling
jarlist = lib/cep-bpmn.jar, \
		  studio/eclipse/plugins/com.tibco.cep.bpmn.rt_0.0.0.jar
          
-include=../../../../../studio/cep.bnd, \
          @lib/cep-bpmn.jar, \
          @studio/eclipse/plugins/com.tibco.cep.bpmn.rt_0.0.0.jar
          
-classpath = lib/cep-bpmn.jar, \
			 studio/eclipse/plugins/com.tibco.cep.bpmn.rt_0.0.0.jar
          
-nouses=true
-sources=false         
Export-Package: *
Include-Resource: !/**/* 
