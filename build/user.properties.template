# Usage Instructions:
#
# 1. Copy this file into user.properties
#
# 2. Edit user.properties to redefine any property default value
#

# =======
# General
# =======

# Root

# Unix
# env.TSI_ROOT=/tsi/root
# env.TSI_ROOT=/local/root

# Windows
# Network:
env.TSI_ROOT=R:
env.SYNC_ROOT=\\\\builder\\root

# Local:
# env.TSI_ROOT=C:/root

# Source Root

PRODUCT=be
BE_BRANCH=5.1

env.SRC_ROOT=${env.TSI_ROOT}/src/DEV/${PRODUCT}/${BE_BRANCH}

# Local Doc Root (needs to be set only if you don't want to use \\potsticker)
# env.DOC_HOME=C:/docs

# Turn off javac compilation warnings about deprecated APIs usage
# javac.deprecation=off

# Build type
env.BUILD_TYPE=dbg
# env.BUILD_TYPE=obf
# env.BUILD_TYPE=rel

# Turn off execution of javadoc tasks
env.JAVADOC_GENERATION=false

# Build Studio plugins with Debug Info
STUDIO_DEBUG_INFO=false

# Enable Packaging for Win64, based on Win32
env.WINDOWS_64BIT_PACKAGING_ENABLE=false

# Enable Packaging for all Unix flavors, based on Linux32
env.UNIX_ALL_PACKAGING_ENABLE=false

# Bypass Obfuscation
#env.BYPASS_OBFUSCATE_ENABLE=true

# For building an engg build testbase. This is equivalent to BE_HOME of an installed version.
ENGG_TESTBASE=C:/testbase/be/5.1

# Development Setup (not used by Build)
env.DEV_SETUP_ROOT=\\\\na-h-filer2\\ql\\tsi-root

# Skip Sync-ing directories (such as docs) which are not needed for compilation. Setting to "false" copies the whole directory.
env.SKIP_SYNC_DIRS=true

# Studio-Tools parameter - OS of machine where target is executed
STUDIO_TOOLS_OS=win

# Studio-Tools parameter - Architecture of machine where target is executed
STUDIO_TOOLS_ARCH=x86_64

# Studio-Tools parameter - Location of BE build area. This could be read-only as well, but needs to be a drive-specified path for Windows
#STUDIO_TOOLS_BE_EXP=R:/tibco/be/5.1.0/L/win/x86

# Studio-Tools parameter - Some temporary directory
#STUDIO_TOOLS_LOCAL_EXP=c:/temp-space

# Studio-Tools parameter - Skip the build of .ear files
STUDIO_TOOLS_BYPASS_BUILD_EAR=true