/**
 * @description 
 */
void rulefunction Queries.CalculateLoanCQCallback {
	attribute {
		validity = ACTION;
	}
	scope {
		String	resultName;
		boolean	isBatchEnd;
		boolean	hasEnded;
		Object	row;
		Object	closure;
	}
	body {
		if (isBatchEnd) {
		    System.debugOut("----Batch:" + resultName + "----");
		} else if (hasEnded) {
		    System.debugOut("----End:" + resultName + "----");
		} else {
		    Object[] columns = row;
		
		    System.debugOut(resultName + " :: " + columns[0]  + ", " +  columns[1] + ", " + columns[2]);
		}
	}
}