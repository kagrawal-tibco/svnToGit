/**
 * @description 
 */
int rulefunction Queries.future {
	attribute {
		alias = "futureRF";
		validity = QUERY;
	}
	scope {
		ConceptModel.Trade	trade;
		ConceptModel.Securities	securities;
	}
	body {
		if ( (securities.inactive == "N") && (trade.settlestatus == "PARTIAL") ) {
		    return trade.loanamtusd;
		}
		
		return 0;
	}
}