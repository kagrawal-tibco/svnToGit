/**
 * @description 
 */
void rulefunction RuleFunctions.PatternResultSuccessHandler {
	attribute {
		validity = ACTION;
	}
	scope {
		String	patternDefURI;
		String	patternInstanceName;
		Object	correlationId;
		Object	closure;
		Object	opaque;
	}
	body {
		RuleFunctions.PatternResultHandler(patternDefURI, patternInstanceName, 
			correlationId, closure, opaque, true);
	}
}