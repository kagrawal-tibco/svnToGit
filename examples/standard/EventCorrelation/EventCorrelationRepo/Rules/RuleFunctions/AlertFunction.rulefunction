/**
 * @description 
 */
void rulefunction Rules.RuleFunctions.AlertFunction {
	attribute {
		validity = ACTION;
	}
	scope {
		
		Events.OutputAlertEvent alert;
	}
	body {
		System.debugOut("__________________________ Sending output alert _______________________________");
		System.debugOut("Alert ID:"+alert.AlertId);
		System.debugOut("Alert Category:"+alert.CategoryId);
		System.debugOut("Alert.Node ID:"+alert.NodeId);

	}
}