/**
 * @description 
 */
boolean rulefunction Functions.haveCommon {
	attribute {
		validity = CONDITION;
	}
	scope {
		Model.Guest	leftGuest;
		Model.Guest	rightGuest;
	}
	body {
		for(int i = 0; i < leftGuest.hobbies@length; i = i+ 1) {
		    for(int j = 0; j < rightGuest.hobbies@length; j = j+ 1) {
		        if(leftGuest.hobbies[i] == rightGuest.hobbies[j])
		            return true;
		    }
		}
		return false;
	}
}