/**
 * @description 
 */
String rulefunction Functions.printGuest {
	attribute {
		validity = ACTION;
	}
	scope {
		Model.Guest	guest;
	}
	body {
		String str = guest.name + "(" + guest.sex + "," + "[";
		
		for(int i = 0; i < guest.hobbies@length; i = i + 1) {
		    if(i!=0)
		        str = str + ", ";
		    str = str + guest.hobbies[i];
		}
		
		return str + "])";
	}
}