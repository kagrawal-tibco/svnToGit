/**
 * @description 
 */
int rulefunction Rules.DivideFunction {
	attribute {
		validity = ACTION;
	}
	scope {
		int	dividend;
		int	divisor;
	}
	body {
		if(divisor == 0)
		{
		    throw newException("DivideByZeroException","Can't divide by zero", null);
		}
		int d = dividend/divisor;
		return d;
	}
}