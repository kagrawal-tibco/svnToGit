rule Rules.Print.PrintEvent {
	attribute {
		priority = 10;
		forwardChain = true;
	}
	declare {
		Events.OrderEvent	order;
		
	}
	when {
		
	}
	then {
		System.debugOut("PrintEvent: Event Received with extId - " + order@extId);
