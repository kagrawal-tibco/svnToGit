<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cluster xmlns="http://tibco.com/businessevents/configuration/5.6">
    <revision>
        <version>69</version>
        <author>TIBCO Software Inc.</author>
        <date>2014/12/03 16:03:53</date>
        <comment/>
    </revision>
    <name>fdsecure</name>
    <message-encoding>UTF-8</message-encoding>
    <object-management>
        <cache-manager>
            <provider>
                <name>TIBCO</name>
            </provider>
            <cache-agent-quorum/>
            <backup-copies>1</backup-copies>
            <entity-cache-size>10000</entity-cache-size>
            <discovery-url>tcp://127.0.0.1:32767</discovery-url>
            <listen-url>tcp://127.0.0.1:32760-*</listen-url>
            <remote-listen-url/>
            <protocol-timeout>-1</protocol-timeout>
            <read-timeout>60000</read-timeout>
            <write-timeout>60000</write-timeout>
            <lock-timeout>-1</lock-timeout>
            <shoutdown-wait>8500</shoutdown-wait>
            <workerthreads-count>4</workerthreads-count>
            <explicit-tuple>true</explicit-tuple>
            <object-table>
                <max-size>100000</max-size>
            </object-table>
            <backing-store>
                <persistence-option>None</persistence-option>
                <cache-aside>true</cache-aside>
                <cache-loader-class/>
                <enforce-pools>true</enforce-pools>
                <strategy>jdbc</strategy>
                <type>Oracle</type>
                <data-store-path/>
                <persistence-policy>ASYNC</persistence-policy>
                <parallel-recovery>true</parallel-recovery>
                <primary-connection>
                    <uri>%%DbUri1%%</uri>
                    <initial-size>4</initial-size>
                    <max-size>8</max-size>
                    <min-size>1</min-size>
                </primary-connection>
            </backing-store>
            <domain-objects>
                <default-mode>cache</default-mode>
                <check-for-version>true</check-for-version>
                <constant>false</constant>
                <enable-tracking>true</enable-tracking>
                <evict-on-update>false</evict-on-update>
                <cache-limited>false</cache-limited>
                <subscribe>true</subscribe>
                <pre-load-enabled>false</pre-load-enabled>
                <pre-load-fetch-size>0</pre-load-fetch-size>
                <pre-load-handles>false</pre-load-handles>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>false</evict-on-update>
                    <mode>memory</mode>
                    <uri>/FraudCriteria</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties>
                            <property>
                                <name>interval</name>
                            </property>
                            <property>
                                <name>numTransactions</name>
                            </property>
                            <property>
                                <name>debitsPercent</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                    <encryption/>
                </domain-object>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>false</evict-on-update>
                    <mode>cache</mode>
                    <uri>/Concepts/Account</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties>
                            <property>
                                <name>PinCode</name>
                            </property>
                            <property>
                                <name>Balance</name>
                            </property>
                            <property>
                                <name>Debits</name>
                            </property>
                            <property>
                                <name>Status</name>
                            </property>
                            <property>
                                <name>AvgMonthlyBalance</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                    <encryption>
                        <property>PinCode</property>
                    </encryption>
                </domain-object>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>false</evict-on-update>
                    <mode>cache</mode>
                    <uri>/Events/AccountOperations</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties>
                            <property>
                                <name>AccountId</name>
                            </property>
                            <property>
                                <name>PinCode</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                    <encryption/>
                </domain-object>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>false</evict-on-update>
                    <mode>cache</mode>
                    <uri>/Events/CreateAccount</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties>
                            <property>
                                <name>AvgMonthlyBalance</name>
                            </property>
                            <property>
                                <name>Balance</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                    <encryption/>
                </domain-object>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>false</evict-on-update>
                    <mode>cache</mode>
                    <uri>/Events/Debit</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties>
                            <property>
                                <name>Amount</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                    <encryption/>
                </domain-object>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>false</evict-on-update>
                    <mode>cache</mode>
                    <uri>/Events/Unsuspend</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties/>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                    <encryption/>
                </domain-object>
            </domain-objects>
            <security-config>
                <enabled>true</enabled>
                <controller>
                    <policy-file>%TIBCO_BE_HOME_ESC%/examples/standard/FraudDetectionSecure/fdpolicy.txt</policy-file>
                    <identity-password>fd$admin</identity-password>
                </controller>
                <requester>
                    <token-file>%TIBCO_BE_HOME_ESC%/examples/standard/FraudDetectionSecure/fdtoken.txt</token-file>
                    <identity-password>fd$admin</identity-password>
                    <certificate-key-file/>
                    <domain-name/>
                    <user-name/>
                    <user-password/>
                </requester>
            </security-config>
        </cache-manager>
        <db-concepts>
            <db-uris/>
            <check-interval>60</check-interval>
            <inactivity-timeout>0</inactivity-timeout>
            <initial-size>5</initial-size>
            <max-size>5</max-size>
            <min-size>5</min-size>
            <property-check-interval>900</property-check-interval>
            <retry-count>-1</retry-count>
            <wait-timeout>1</wait-timeout>
        </db-concepts>
    </object-management>
    <rulesets>
        <rules id="all-rules">
            <uri>/</uri>
        </rules>
    </rulesets>
    <function-groups>
        <functions id="all-functions">
            <uri>/</uri>
        </functions>
    </function-groups>
    <destination-groups>
        <destinations id="Http">
            <destination id="AllOperations">
                <pre-processor>/RuleFunctions/PreProcessor</pre-processor>
                <queue-size>0</queue-size>
                <thread-count/>
                <threading-model>caller</threading-model>
                <thread-affinity-rule-function/>
                <uri>/Channels/HTTP/AllOps</uri>
            </destination>
        </destinations>
    </destination-groups>
    <process-groups/>
    <log-configs>
        <log-config id="logConfig">
            <enabled>true</enabled>
            <roles>*:info</roles>
            <files>
                <enabled>true</enabled>
                <dir>logs</dir>
                <name/>
                <max-number>10</max-number>
                <max-size>5000000</max-size>
                <append>true</append>
            </files>
            <terminal>
                <enabled/>
                <sys-out-redirect/>
                <sys-err-redirect/>
                <encoding/>
            </terminal>
            <line-layout>
                <enabled>false</enabled>
                <class/>
                <arg/>
            </line-layout>
        </log-config>
    </log-configs>
    <agent-classes>
        <cache-agent-class id="cache-class">
            <property-group/>
        </cache-agent-class>
        <inference-agent-class id="inference-class">
            <rules>
                <ref>all-rules</ref>
            </rules>
            <destinations>
                <ref>Http</ref>
            </destinations>
            <startup>
                <uri>/RuleFunctions/InitializeScorecard</uri>
            </startup>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size/>
                    <max-time/>
                </eviction>
            </local-cache>
            <shared-queue>
                <size>1024</size>
                <workers>10</workers>
            </shared-queue>
            <load>
                <max-active/>
            </load>
            <concurrent-rtc>false</concurrent-rtc>
            <check-for-duplicates>false</check-for-duplicates>
            <businessworks>
                <uri/>
            </businessworks>
            <property-group/>
        </inference-agent-class>
    </agent-classes>
    <processing-units>
        <processing-unit id="cache">
            <agents>
                <agent>
                    <ref>cache-class</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
            <security-config>
                <role>Controller</role>
                <controller>
                    <policy-file override="false"/>
                    <identity-password override="false"/>
                </controller>
                <requester>
                    <token-file override="false"/>
                    <identity-password override="false"/>
                    <certificate-key-file override="false"/>
                    <domain-name override="false"/>
                    <user-name override="false"/>
                    <user-password override="false"/>
                </requester>
            </security-config>
        </processing-unit>
        <processing-unit id="default">
            <agents>
                <agent>
                    <ref>inference-class</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>false</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
            <security-config>
                <role>Requester</role>
                <controller>
                    <policy-file override="false"/>
                    <identity-password override="false"/>
                </controller>
                <requester>
                    <token-file override="false"/>
                    <identity-password override="false"/>
                    <certificate-key-file override="false"/>
                    <domain-name override="false"/>
                    <user-name override="false"/>
                    <user-password override="false"/>
                </requester>
            </security-config>
        </processing-unit>
    </processing-units>
    <load-balancer-configs>
        <pair-configs/>
        <adhoc-configs/>
    </load-balancer-configs>
    <property-group>
        <property-group comment="" name="auth">
            <property name="be.mm.auth.type" value="file"/>
            <property name="be.mm.auth.file.location" value="%TIBCO_BE_HOME_ESC%/mm/config/users.pwd"/>
            <property name="java.security.auth.login.config" value="%TIBCO_BE_HOME_ESC%/mm/config/jaas-config.config"/>
        </property-group>
    </property-group>
</cluster>
