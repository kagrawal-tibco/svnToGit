/**
 * @description 
 * @author 
 */
rule Rules.PrintConcept {
	
	attribute {
		priority = 5;
		forwardChain = true;
	}
	
	declare {
		Concepts.Data	d;
	}
	
	when {
		Instance.isNew(d);
	}
	
	then {
		System.debugOut("PrintConcept: New Concept with @extId='"+ d@extId + "', Text='"+ d.Text + "'.");
	}
	
}