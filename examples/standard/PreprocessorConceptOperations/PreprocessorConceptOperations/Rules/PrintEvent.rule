/**
 * @description 
 * @author 
 */
rule Rules.PrintEvent {
	
	attribute {
		priority = 5;
		forwardChain = true;
	}
	
	declare {
		Events.Action	action;
	}
	
	when {
	}
	
	then {
		System.debugOut("PrintEvent: Event received with Action='" + action.Action
			+ "', Identifier='" + action.Identifier
			+ "', Parameter='" + action.Parameter
			+ "'.");
	}
	
}