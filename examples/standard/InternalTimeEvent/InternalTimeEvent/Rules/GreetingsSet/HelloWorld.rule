/**
 * @description Every time the InternalTimeEvent is asserted, this rule prints a message to the console.
 * @author 
 */
rule Rules.GreetingsSet.HelloWorld {
	attribute {
		priority = 1;
		forwardChain = true;
	}
	declare {
		Events.InternalTimeEvent	internaltimeevent;
		
	}
	when {
		
	}
	then {
		System.debugOut("############### Hello world every 5 seconds, from the scheduled time event ###############");
	}
}