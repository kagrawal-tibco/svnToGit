/**
 * @description When the Startup rule function asserts the RuleBasedTimeEvent event, this rule prints a message to the console.
 * @author 
 */
rule Rules.GreetingsSet.HelloWorldForRuleBasedTimeEvent {
	attribute {
		priority = 5;
		forwardChain = true;
	}
	declare {
		
		Events.RuleBasedTimeEvent rulebasedtimeevent;
	}
	when {
		
	}
	then {
		System.debugOut("############### Hello world one time only, from the rule based time event ###############");
	}
}