rule Rules.PrintRule {
	
	attribute {
		priority = 5;
		forwardChain = true;
	}
	
	declare {
		Events.TextEvent	textevent;
	}

	when {
	}

	then {
		System.debugOut("PrintRule received TextEvent: " + textevent.Text);

		RuleFunctions.CloseHttpRequest(textevent);
		Event.consumeEvent(textevent);
	}

}