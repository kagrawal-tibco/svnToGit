                                <name>name</name>
                            </property>
                            <property>
                                <name>account</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                </domain-object>
                <domain-object>
                    <mode>cache</mode>
                    <uri>/Concepts/Order</uri>
                    <backing-store>
                        <enabled>true</enabled>
                        <table-name/>
                        <properties>
