     <extension
        point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
           class="com.tibco.cep.studio.decision.table.constraintpane.DecisionTableAnalysisProblemResolutionProvider"
           markerType="com.tibco.cep.decision.table.ui.analyzeProblem">
      </markerResolutionGenerator>
