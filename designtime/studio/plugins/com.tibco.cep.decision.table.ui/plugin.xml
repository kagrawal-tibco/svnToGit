   <extension
        id="analyzeProblem"
        name="Decision table Analyze Problems"
        point="org.eclipse.core.resources.markers">
      <super
           type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
           value="false">
      </persistent>
   </extension>
    <extension
         id="syntaxProblem"
         name="Decision table Syntax Errors"
         point="org.eclipse.core.resources.markers">
       <super
            type="com.tibco.cep.studio.core.validationProblem">
       </super>
       <persistent
            value="true">
       </persistent>
       <attribute
            name="com.tibco.cep.decision.table.trs">
       </attribute>
       <attribute
            name="com.tibco.cep.decision.table.trv">
       </attribute>
    </extension> 
     <extension
        id="analyzeCombinationProblem"
        name="Decision table Rule Combination Problem"
        point="org.eclipse.core.resources.markers">
      <super
           type="com.tibco.cep.decision.table.ui.analyzeProblem">
      </super>
      <persistent
           value="false">
      </persistent>
   </extension>
     <extension
        id="analyzeGeneralProblem"
        name="Decision table Rule Problem"
        point="org.eclipse.core.resources.markers">
      <super
           type="com.tibco.cep.decision.table.ui.analyzeProblem">
      </super>
      <persistent
           value="false">
      </persistent>
   </extension>
