packageFrameTemplate(cssPath,dataObject) ::= <<
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!--NewPage-->
<HTML>
	<HEAD>
		<meta http-equiv="content-type" content="text/html; charset=UTF-8">
		<TITLE></TITLE>
		<LINK REL ="stylesheet" TYPE="text/css" HREF="$cssPath$" TITLE="Style">
	</HEAD>
	<BODY BGCOLOR="white">
		$if(dataObject.packageName)$
			<FONT size="+1" CLASS="FrameTitleFont">
				<a href="package-summary.html" target="classFrame">$dataObject.packageName$</a>
			</FONT><BR/>
		$endif$
		$if(dataObject.subProcesses)$
			<table width="100%" border="0" summary=""><tbody>
				<tr><td nowrap="">
					<font size="+1" class="FrameHeadingFont">Sub Processes</font>&nbsp;
					<font class="FrameItemFont">
						$dataObject.subProcesses: {e|<br/><a href="$e.link$" target="$e.target$" title="$e.title$"><nobr>$e.name$</nobr></a>
						}$
					</font>
				</tb></tr>
			</tbody></table>
		$endif$
		$if(dataObject.events)$
			<table width="100%" border="0" summary=""><tbody>
				<tr><td nowrap="">
					<font size="+1" class="FrameHeadingFont">Events</font>&nbsp;
					<font class="FrameItemFont">
						$dataObject.events: {e|<br/><a href="$e.link$" target="$e.target$" title="$e.title$"><nobr>$e.name$</nobr></a>
						}$
					</font>
				</tb></tr>
			</tbody></table>
		$endif$
		$if(dataObject.elements)$
			<table width="100%" border="0" summary=""><tbody>
				<tr><td nowrap="">
					<font size="+1" class="FrameHeadingFont">Elements</font>&nbsp;
					<font class="FrameItemFont">
						$dataObject.elements: {e|<br/><a href="$e.link$" target="$e.target$" title="$e.title$"><nobr>$e.name$</nobr></a>
						}$
					</font>
				</tb></tr>
			</tbody></table>
		$endif$
		$if(dataObject.sequences)$
			<table width="100%" border="0" summary=""><tbody>
				<tr><td nowrap="">
					<font size="+1" class="FrameHeadingFont">Sequences</font>&nbsp;
					<font class="FrameItemFont">
						$dataObject.sequences: {e|<br/><a href="$e.link$" target="$e.target$" title="$e.title$"><nobr>$e.name$</nobr></a>
						}$
					</font>
				</tb></tr>
			</tbody></table>
		$endif$
	</BODY>
</HTML>
>>

