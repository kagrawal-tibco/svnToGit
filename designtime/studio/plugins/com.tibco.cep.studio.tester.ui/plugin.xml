                    style="push"
                    tooltip="Start Test">
          </command>
       </toolbar>
    </menuContribution-->
 </extension>
 <extension
       point="org.eclipse.ui.commands">
    <command
          defaultHandler="com.tibco.cep.studio.tester.ui.actions.ShowWMActionDelegate"
          description="Show working memory entities"
          id="com.tibco.cep.studio.tester.ui.showWMCommand"
          name="Show Working Memory">
    </command>
 </extension>
 <extension
       point="org.eclipse.ui.actionSets">
    <actionSet
          id="com.tibco.cep.studio.tester.ui.actionSet"
          label="Tester"
          visible="true">
       <action
             class="com.tibco.cep.studio.tester.ui.actions.ShowWMActionDelegate"
             icon="icons/wm_17x16.png"
             id="com.tibco.cep.studio.tester.ui.showWMResultsaction"
             label="Working Memory Contents"
             style="pulldown"
             toolbarPath="Normal/additions">
       </action>
    </actionSet>
 </extension>
 <extension
       point="org.eclipse.ui.preferencePages">
    <page
          category="com.tibco.cep.studio.tester.testerPreferencePage"
          class="com.tibco.cep.studio.tester.ui.preferences.TesterAppearancePreferencePage"
          id="com.tibco.cep.studio.tester.ui.testAppearancePage"
          name="Appearance">
    </page>
 </extension>
 <extension
       point="org.eclipse.core.runtime.preferences">
    <initializer
          class="com.tibco.cep.studio.tester.ui.preferences.TesterPreferenceInitializer">
    </initializer>
 </extension>
 <extension
       point="com.tibco.cep.studio.debug.ui.debugInputTab">
    <InputTab
          Tab="com.tibco.cep.studio.tester.ui.view.TesterInputTab">
    </InputTab>
 </extension>
 <extension
       point="org.eclipse.ui.newWizards">
    <wizard
          category="com.tibco.cep.studio.ui.category"
          class="com.tibco.cep.studio.tester.ui.wizards.NewBEUnitTestWizard"
          icon="icons/test.gif"
          id="com.tibco.cep.studio.tester.ui.wizards.NewBEUnitTestWizard"
          name="BEUnit Test Suite">
          <description>
            Create a new BEUnit Test Suite.
         </description>
    </wizard>
 </extension>
</plugin>
