 <extension
       point="org.eclipse.ui.menus">
    <menuContribution
          locationURI="menu:project?after=additions">
          <menu
                icon="icons/wm_17x16.png"
                id="com.tibco.cep.studio.tester.ui.showWMResultsMenuCommand"
                label="Show Working Memory Contents">
             <dynamic
                   class="com.tibco.cep.studio.tester.ui.actions.ShowWMDynamicContribution"
                   id="com.tibco.cep.studio.tester.ui.showWMresults.dynamic">
             </dynamic>
          </menu>
    </menuContribution>
    <!--menuContribution
          locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
       <toolbar
