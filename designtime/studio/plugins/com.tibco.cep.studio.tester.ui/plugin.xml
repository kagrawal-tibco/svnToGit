<plugin>
<extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.tibco.cep.studio.tester.ui.editor.data.TestDataEditor"
            contributorClass="com.tibco.cep.studio.tester.ui.editor.data.TestEditorContributor"
            default="true"
            extensions="concepttestdata"
            icon="icons/conceptTestData.png"
            id="com.tibco.cep.studio.tester.ui.editor.data.concepttestdataeditor"
            name="Concept Test Data Editor">
      </editor>
        <editor
            class="com.tibco.cep.studio.tester.ui.editor.data.TestDataEditor"
            contributorClass="com.tibco.cep.studio.tester.ui.editor.data.TestEditorContributor"
            default="false"
            extensions="eventtestdata"
            icon="icons/eventTestData.png"
            id="com.tibco.cep.studio.tester.ui.editor.data.eventtestdataeditor"
            name="Event Test Data Editor">
      </editor>    
     <editor
            class="com.tibco.cep.studio.tester.ui.editor.data.TestDataEditor"
            contributorClass="com.tibco.cep.studio.tester.ui.editor.data.TestEditorContributor"
            default="true"
            extensions="scorecardtestdata"
            icon="icons/scorecardTestData.png"
            id="com.tibco.cep.studio.tester.ui.editor.data.scorecardtestdataeditor"
            name="Scorecard Test Data Editor">
      </editor>
      <editor
            class="com.tibco.cep.studio.tester.ui.editor.result.TestResultEditor"
            default="false"
            extensions="resultdata"
            icon="icons/test.gif"
            id="com.tibco.cep.studio.tester.ui.result.editor"
            name="Test Result">
      </editor>
      <editor
            class="com.tibco.cep.studio.tester.ui.editor.result.WMResultEditor"
            default="false"
            icon="icons/wm_17x16.png"
            id="com.tibco.cep.studio.tester.ui.wm.result.editor"
            name="WM Result">
      </editor>
   </extension>
