   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonFilter
            description="Hide Non Studio projects"
            id="com.tibco.cep.studio.projectexplorer.commonFilter2"
            name="Non Studio Projects">
         <filterExpression>
            <not>
               <adapt
                     type="org.eclipse.core.resources.IProject">
                  <test
                        property="org.eclipse.core.resources.projectNature"
                        value="com.tibco.cep.studio.project.nature">
                  </test>
               </adapt>
            </not>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            description="Hide Global Variables"
            id="com.tibco.cep.studio.projectexplorer.globalvarfilter"
            name="Global Variables">
         <filterExpression>
            <or>
               <adapt type="org.eclipse.core.resources.IResource">
                  <test property="org.eclipse.core.resources.name"
                        value="*.substvar">
                  </test>
               </adapt>
			   <adapt type="org.eclipse.core.resources.IFolder">
                  <test property="org.eclipse.core.resources.name"
                        value="defaultVars">
                  </test>
               </adapt>
            </or>
         </filterExpression>
      </commonFilter>      
