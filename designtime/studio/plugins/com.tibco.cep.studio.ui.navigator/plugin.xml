<plugin>
   <extension-point id="staticViewerFilter" name="Static Studio Explorer Filter" schema="schema/staticViewerFilter.exsd"/>
 <extension
         point="org.eclipse.ui.views">
      <view
            category="com.tibco.cep.studio.ui.category"
            class="com.tibco.cep.studio.ui.navigator.view.ProjectExplorer"
            icon="icons/navigation.gif"
            id="com.tibco.cep.studio.projectexplorer.view"
            name="Studio Explorer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="com.tibco.cep.studio.projectexplorer.view">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
            <actionExtension
                  pattern="com.tibco.cep.studio.ui.navigator.providers.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="com.tibco.cep.studio.projectexplorer.view">
         <includes>
            <contentExtension pattern="org.eclipse.ui.navigator.resources.filters.*"/>
            <contentExtension pattern="org.eclipse.ui.navigator.resources.linkHelper"/>
            <contentExtension pattern="org.eclipse.ui.navigator.resources.workingSets"/>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.conceptContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.stateMachineContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.ruleSetContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.channelContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.eventNavigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.earContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.ui.navigator.propertyNavigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.ui.navigator.eventPropertyNavigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.ui.navigator.NRContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.ui.navigator.StateEntitynavigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.vrfnavigatorcontent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension pattern="org.eclipse.ui.navigator.resourceContent"/>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.globalvarfilter">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.linkedprojlibresourcefilter">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.decision.table.navigator.content">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.decision.table.arguments.navigator.content">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.dashboard.metrictContent">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.projectlib.globalvarfilter">
            </contentExtension>
            <contentExtension
                  pattern="com.tibco.cep.studio.projectexplorer.javaelementfilter">
            </contentExtension>
         </includes>
      </viewerContentBinding>
