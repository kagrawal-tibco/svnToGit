  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint -p tsconfig.json -c tslint.json --exclude '**/dto.ts'",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build",
    "precommit": "lint-staged && unset GIT_DIR && if [ \"$(git diff --cached --name-status | awk '$1 != \"D\" { print $2 }' | grep '^sb-rms/src/main/ams-web/src' -c)\" != \"0\" ]; then ng build --aot=true; fi"
  },
  "lint-staged": {
    "gitDir": "../../../..",
    "concurrent": true,
    "linters": {
      "sb-rms/src/main/ams-web/src/**/*.ts": [
        "tslint --project tsconfig.json --config tslint.json --exclude '**/dto.ts'"
      ]
    }
  },
  "private": true,
