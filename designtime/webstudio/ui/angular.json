{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tibco-ams": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/animate.css/animate.min.css",
              "node_modules/jquery-contextmenu/dist/jquery.contextMenu.min.css",
              "node_modules/ag-grid-community/dist/styles/ag-grid.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-fresh.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-bootstrap.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-blue.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-material.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-dark.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham-dark.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/angular2-toaster/toaster.css",
              "node_modules/xterm/dist/xterm.css",
              "node_modules/bootstrap-social/bootstrap-social.css",
              "node_modules/codemirror/lib/codemirror.css",
              "node_modules/codemirror/addon/merge/merge.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/jquery-contextmenu/dist/jquery.contextMenu.min.js",
              "node_modules/xterm/dist/xterm.js",
              "node_modules/xterm/dist/addons/fit/fit.js",
              "node_modules/codemirror/lib/codemirror.js"
            ]
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/./environments/environment.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/./environments/environment.prod.ts"
                }
              ]
            },
            "be": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/./environments/environment.be.ts"
                }
              ]
            },
            "tce": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/./environments/environment.tce.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tibco-ams:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "tibco-ams:build:dev"
            },
            "production": {
              "browserTarget": "tibco-ams:build:production"
            },
            "be": {
              "browserTarget": "tibco-ams:build:be"
            },
            "tce": {
              "browserTarget": "tibco-ams:build:tce"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tibco-ams:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/jquery-contextmenu/dist/jquery.contextMenu.min.js",
              "node_modules/xterm/dist/xterm.js",
              "node_modules/xterm/dist/addons/fit/fit.js",
              "node_modules/xterm/dist/addons/linkify/linkify.js",
              "node_modules/codemirror/lib/codemirror.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/animate.css/animate.min.css",
              "node_modules/jquery-contextmenu/dist/jquery.contextMenu.min.css",
              "node_modules/ag-grid-community/dist/styles/ag-grid.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-fresh.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-bootstrap.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-blue.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-material.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-dark.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham-dark.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/angular2-toaster/toaster.css",
              "node_modules/xterm/dist/xterm.css",
              "node_modules/bootstrap-social/bootstrap-social.css",
              "node_modules/codemirror/lib/codemirror.css",
              "node_modules/codemirror/addon/merge/merge.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "tibco-ams-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "tibco-ams:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "tibco-ams",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}