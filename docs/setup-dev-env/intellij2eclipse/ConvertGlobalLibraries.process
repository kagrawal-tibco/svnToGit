<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/ns/no_namespace_schema_location/IntellijSchema.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pfx2="http://www.tibco.com/ns/no_namespace_schema_location/eclipseSchema.xsd" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/IntellijSchema.xsd" schemaLocation="/IntellijSchema.xsd"/>
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/eclipseSchema.xsd" schemaLocation="/eclipseSchema.xsd"/>
    <pd:name>ConvertGlobalLibraries.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Convert">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="OutputFile" type="xsd:string"/>
                    <xsd:element name="InputFile" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>107</pd:startX>
    <pd:startY>75</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>772</pd:endX>
    <pd:endY>76</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:activity name="Read File">
        <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
        <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
        <pd:x>255</pd:x>
        <pd:y>77</pd:y>
        <config>
            <encoding>text</encoding>
        </config>
        <pd:inputBindings>
            <ns2:ReadActivityInputClass>
                <fileName>
                    <xsl:value-of select="$Start/Convert/InputFile"/>
                </fileName>
                <encoding>
                    <xsl:value-of select="&quot;UTF-8&quot;"/>
                </encoding>
            </ns2:ReadActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>372</pd:x>
        <pd:y>78</pd:y>
        <config>
            <inputStyle>binary</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx:application"/>
        </config>
        <pd:inputBindings>
            <xmlBinary>
                <bytes>
                    <xsl:value-of select="$Read-File/ns:ReadActivityOutputTextClass/fileContent/textContent"/>
                </bytes>
                <xsl:if test="$Read-File/ns:ReadActivityOutputTextClass/fileContent/encoding">
                    <forceEncoding>
                        <xsl:value-of select="$Read-File/ns:ReadActivityOutputTextClass/fileContent/encoding"/>
                    </forceEncoding>
                </xsl:if>
            </xmlBinary>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write File">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>661</pd:x>
        <pd:y>75</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
        </config>
        <pd:inputBindings>
            <ns:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="$Start/Convert/OutputFile"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="$Render-XML/xmlString"/>
                </textContent>
                <xsl:if test="$Read-File/ns:ReadActivityOutputTextClass/fileContent/encoding">
                    <encoding>
                        <xsl:value-of select="$Read-File/ns:ReadActivityOutputTextClass/fileContent/encoding"/>
                    </encoding>
                </xsl:if>
            </ns:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>515</pd:x>
        <pd:y>77</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx2:eclipse-userlibraries"/>
        </config>
        <pd:inputBindings>
            <eclipse-userlibraries>
                <xsl:attribute xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="version">
                    <xsl:value-of select="2"/>
                </xsl:attribute>
                <xsl:for-each select="$Parse-XML/application/component/library">
                    <library>
                        <xsl:if test="@name">
                            <xsl:attribute name="name">
                                <xsl:value-of select="@name"/>
                            </xsl:attribute>
                        </xsl:if>
                        <xsl:for-each select="CLASSES/root">
                            <archive>
                                <xsl:if test="tib:substring-before-last(substring-after(@url, &quot;jar://&quot;), &quot;!/&quot;)">
                                    <xsl:attribute name="path">
                                        <xsl:value-of select="tib:substring-before-last(substring-after(@url, &quot;jar://&quot;), &quot;!/&quot;)"/>
                                    </xsl:attribute>
                                </xsl:if>
                                <xsl:if test="../../SOURCES/root/@url">
                                    <xsl:attribute name="source">
                                        <xsl:value-of select="../../SOURCES/root/@url"/>
                                    </xsl:attribute>
                                </xsl:if>
                                <xsl:if test="../../JAVADOC/root/@url">
                                    <xsl:attribute name="javadoc">
                                        <xsl:value-of select="../../JAVADOC/root/@url"/>
                                    </xsl:attribute>
                                </xsl:if>
                                <xsl:value-of select=""/>
                            </archive>
                        </xsl:for-each>
                    </library>
                </xsl:for-each>
            </eclipse-userlibraries>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Read File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Read File</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write File</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>Write File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>