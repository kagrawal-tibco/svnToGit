<configuration>

	<!-- Enable for more detailed error messages -->
	<debugMode>false</debugMode>

	<!-- If demo mode is enabled, chart updates are faked with random values based on the most recent value -->
	<demoMode>false</demoMode>
	
	<!-- Defines the time interval in seconds between server update queries -->
	<updateInterval>5</updateInterval>
	
	<!-- Maximum ratio of failed pane updates to number of displayed panes before system crash assumed -->
	<failedPaneThreshold>1.0</failedPaneThreshold>
	
	<!-- URL for the company logo that's displayed in the upper left corner -->
