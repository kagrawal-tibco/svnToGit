				<method name="GetNumberOfInstances"/>
				<method name="Resume"/>
				<method name="Suspend"/>
				<!-- <method name="purgeDeletedObjects"/>
				<method name="printL1CacheContents"/>
				<method name="printLockManagerDetails"/>
				<method name="printAsynchronousWorkerDetails"/>
				<method name="printMemoryInfo"/> -->
