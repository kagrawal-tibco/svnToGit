/**
 * @description 
 */
Shared.Concepts.MonitorableEntity rulefunction Shared.RuleFunctions.topology.getChildByName {
	attribute {
		validity = ACTION;
	}
	scope {
		
		String name;
		Shared.Concepts.MonitorableEntity parent;
	}
	body {
		MonitorableEntity child = null;
		for(int i=0;i<parent.children@length;i=i+1){
			child = parent.children[i];
			if(String.equals(name, child.name)){
				return child;
			}
		}
		return null;
	}
}