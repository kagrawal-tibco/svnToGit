/**
 * @description 
 */
String rulefunction Monitoring.RuleFunctions.getClustHealthConcptExtId {
	attribute {
		validity = ACTION;
	}
	scope {
		String clustName;
		
	}
	body {
		if (clustName == null || String.trim(clustName) == "")
			return"";
		
		return clustName+"_clust_health";  //with a good chance it will be a unique ExtId 
	}
}