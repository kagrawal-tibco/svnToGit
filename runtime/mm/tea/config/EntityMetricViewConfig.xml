   <section sectionId="3" displayName="Rule Statistics">
      <chart>
         <id>13</id>
         <chartType>column</chartType>
         <entity>agent</entity>
         <name>executionTimeChart</name>
         <description>chart which avg rule execution time per rule</description>
         <displayName>Rule Execution Time</displayName>
         <isBucketed>false</isBucketed>
         <threshold>0</threshold>
         <xConfig>
            <type />
            <title>Rule Name</title>
            <precision>0</precision>
         </xConfig>
         <yConfig>
            <type>number</type>
            <title>Average Execution Time(millis)</title>
            <precision>2</precision>
         </yConfig>
         <series>
            <name>execTimeSeries</name>
            <displayName>Rule Execution Time</displayName>
            <cube>BEMMCube</cube>
            <hierarchy>rulehealthhierarchy</hierarchy>
            <dimLevel>rule</dimLevel>
            <filters />
            <xAttribute>rule</xAttribute>
            <yAttribute>avgexecutiontime</yAttribute>
            <orderBy>avgexecutiontime</orderBy>
            <sortOrder>DESCENDING</sortOrder>
            <!--  [ASCENDING,DESCENDING ,ascending,descending] defaults to ascending -->
         </series>
         <poller>
            <pollingInterval>300</pollingInterval>
            <maxPolls>5</maxPolls>
         </poller>
      </chart>
   </section>
