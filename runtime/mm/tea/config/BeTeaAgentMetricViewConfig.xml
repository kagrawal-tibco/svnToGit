<?xml version="1.0" encoding="UTF-8"?>
<MetricsViewConfig xmlns="http://www.tibco.com/cep/bemm/monitoring/metric/view/config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.tibco.com/cep/bemm/monitoring/metric/view/config ../xsds/EntityMetricsVisualizationConfig.xsd ">
   <!-- EVERY CHART MUST HAVE A UNIQUE CHART ID -->
   <section sectionId="1" displayName="Five Minute Statistics">
      <chart>
         <id>1</id>
         <chartType>spline</chartType>
         <entity>beteaagent</entity>
         <name>avgmemusagechart</name>
         <description>chart which displays used Memory versus minute</description>
         <displayName>Average Used Memory</displayName>
         <isBucketed>true</isBucketed>
         <threshold>7200</threshold>
         <timeDimension>minutes</timeDimension>
         <xConfig>
            <type>time</type>
            <title>Time</title>
            <precision>0</precision>
         </xConfig>
         <yConfig>
            <type>percent</type>
            <title>%</title>
            <precision>2</precision>
         </yConfig>
         <series>
            <name>AvgMemUsageSeries</name>
            <displayName>Average Used Memory</displayName>
            <schema>BETEA</schema>
            <cube>SystemStats</cube>
            <hierarchy>systemstatshierarchy</hierarchy>
            <dimLevel>minutes</dimLevel>
            <filters />
            <xAttribute>timestamp</xAttribute>
            <yAttribute>avgmemusage</yAttribute>
         </series>
         <poller>
            <pollingInterval>300</pollingInterval>
            <maxPolls>5</maxPolls>
         </poller>
      </chart>
