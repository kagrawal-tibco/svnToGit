###############################################################
##
## BE-TEA Agent Property File
##
###############################################################

# TEA Server URL
# Mandatory  Defaults as below
be.tea.server.url=http://localhost:8777/tea

# TEA Agent Listen Port 
# Mandatory Defaults as below
be.tea.agent.port=9777

# TEA agent auto-registration retry interval (in milliseconds)
# Optional. Defaults as below.
#be.tea.agent.retry.interval=5000

# Path to the 'ui' directory containing the TEA agent web resources 
# Mandatory
be.tea.agent.resource.base=%TIBCO_BE_HOME_ESC%/teagent/

# TEA Agent Deployments configuration DataStore
be.tea.agent.application.datastore=%TIBCO_BE_HOME_ESC%/teagent/repo

# Monitoring metric collector plugins config XML
# Do not change
be.tea.agent.rta.config.plugin.dir=%TIBCO_BE_HOME_ESC%/teagent/config/plugins

# Monitoring Views config XML file location 
# Do not change
be.tea.agent.metrics.view.config.file=%TIBCO_BE_HOME_ESC%/teagent/config/EntityMetricViewConfig.xml

# Self Monitoring Views config XML file location 
# Do not change
be.tea.agent.self.metrics.view.config.file=%TIBCO_BE_HOME_ESC%/teagent/config/BeTeaAgentMetricViewConfig.xml

# Monitoring Be Entity Config file
# Do not change
be.tea.agent.view.beentity.config.file=%TIBCO_BE_HOME_ESC%/teagent/config/BeEntityMap.xml

#Rules Attribute-entity mapping config XML file location 
# Do not change
be.tea.agent.metrics.rules.attr.map.file=%TIBCO_BE_HOME_ESC%/teagent/config/RuleEntityAttrMap.xml

# Location on the file system where the server will load the schema files.
# Do not change
be.tea.agent.schema.store=%TIBCO_BE_HOME_ESC%/teagent/config/schema
#

#JMX port for the BE Tea Agent
#Defaults to 5566
be.tea.agent.jmx.port=5566
#

#Enable use of single port for JMX connection
#Optional Defaults as below
#be.tea.agent.jmx.usesingleport=true
#

#Enable SSL for JMX Connection
#Optional Defaults as below
#be.tea.agent.jmx.usessl=false
#

# Application instances status poller interval (in milliseconds)
# Optional Defaults as below
#be.tea.agent.poller.delay=30000

# Enable additional views (Garbage collector and Memory Pool chart) 
# Default is set to false
# be.tea.agent.enable.gc.charts=false

#
# Maximum number of worker threads for thread pools.
#
# Optional, default as below.
#be.tea.agent.worker.thread.count=16
#	

#
# The minimum number of threads to retain in the thread pools even at non-peak loads.
#
# Optional, default as below. Keep it less than or equal to be.tea.agent.worker.thread.count
#be.tea.agent.worker.thread.count.min=1
#

#
# Time interval in seconds after which idle threads of the thread pools will be stopped, till the thread count
# reaches the be.tea.agent.worker.thread.count.min setting.
# 
# Optional, default as below.
#be.tea.agent.worker.thread.idle.timeout=300
#

#
# Job queue size of various thread pools.
# Keep it low, since unless the queue is full no new threads will be added to the worker thread pool.
# Optional, default as below.
#be.tea.agent.worker.queue.size=4
#

#
#
# Number of threads to perform the metrics computation jobs.
#
# Optional, default as below. Do not reduce to less than the number of hierarchies as defined in the schema.
#be.tea.agent.metric.compute.thread.count=32
#

	
#
#
# Job queue size for the metrics computation jobs thread pool.
#
# As a norm, set it to twice that value of be.tea.agent.sticky.worker.thread.count.
# Optional, default as below. 
#be.tea.agent.metric.compute.queue.size=64
#


#
#
# Time interval in milliseconds, to scan for scheduled actions.
#
# Optional, default as below.
#be.tea.agent.rules.actions.scan.frequency=5000
#


##############################################
#
# Actions Configurations
#
##############################################
# Alert Log format i.e. XML or TEXT
# Optional, default is TEXT
#be.tea.agent.log.alert.format=TEXT
#

##############################################
#
# Email Action Configuration
#
##############################################

#
#
# SMTP server host name
# MANDATORY
be.tea.agent.mail.smtp.host=
#

#
#
# SMTP server port
#
#Optional, default as below. 
#be.tea.agent.mail.smtp.port=25
#

#
#
# Whether SMTP server authentication is required or not.
#
#Optional, default as below. 
#be.tea.agent.mail.smtp.authentication=false
#

#
#
# SMTP server user name
#
# Required only if SMTP server authentication is true.
#be.tea.agent.mail.smtp.user=
#

#
#
# SMTP server user password.
#
# Required only if SMTP server authentication is true.
#be.tea.agent.mail.smtp.password=
#

#
# Email From (Email Sender)
# 
#Optional, default current system user
#be.tea.agent.mail.from=
#

#
# Email Retry count.
# 
#Optional, default is 3. -1 will retry infinitely.
#be.tea.agent.mail.retry.count=3
#

#
# Email Retry interval (milliseconds).
# 
#Optional, default is 2000 milliseconds
#be.tea.agent.mail.retry.interval=2000
#

##############################################
#
# SSH configurations
#
##############################################
 
# Path to the private key file for password-less ssh authentication
#be.tea.agent.ssh.privatekey.file=

# Passphrase to the private key file (if required)
#be.tea.agent.ssh.privatekey.passphrase=

# SSH connection timeout
#be.tea.agent.ssh.connection.timeout=30000

# Location on the file system where the server will load the message file.
be.tea.agent.message.file=%TIBCO_BE_HOME_ESC%/teagent/config/messages/messages_en_US.properties
#