/**
 * @description Append string[] to a buffer and return it.
 */
Object rulefunction Approval.CommonServices.AMS_RF_JoinStringsToBuffer {
	attribute {
		validity = ACTION;
	}
	scope {
		String[] stringArray;
		Object buffer;
	}
	body {
		for (int loop = 0; loop < stringArray@length; loop++) {
			String.append(buffer, stringArray[loop]);
		}
		return buffer;
	}
}