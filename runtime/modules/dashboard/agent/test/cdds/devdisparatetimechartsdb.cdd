<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cluster xmlns="http://tibco.com/businessevents/configuration/5.1">
    <revision>
        <version>6</version>
        <author>TIBCO Software Inc.</author>
        <date>2011/03/17 12:07:11</date>
        <comment/>
    </revision>
    <name>disparatetimecharts-anpatil-dt</name>
    <message-encoding>UTF-8</message-encoding>
    <object-management>
        <cache-manager>
            <provider>
                <name>TIBCO</name>
            </provider>
            <cache-agent-quorum/>
            <backup-copies>1</backup-copies>
            <entity-cache-size>10000</entity-cache-size>
            <object-table>
                <max-size>100000</max-size>
            </object-table>
            <backing-store>
                <persistence-option>None</persistence-option>
                <cache-aside>true</cache-aside>
                <cache-loader-class/>
                <enforce-pools>true</enforce-pools>
                <strategy>jdbc</strategy>
                <type>Oracle</type>
                <data-store-path/>
                <persistence-policy>ASYNC</persistence-policy>
                <primary-connection>
                    <uri>/SharedResources/JDBCConfig.sharedjdbc</uri>
                    <initial-size>4</initial-size>
                    <max-size>8</max-size>
                    <min-size>1</min-size>
                </primary-connection>
            </backing-store>
            <domain-objects>
                <default-mode>cache</default-mode>
                <check-for-version>true</check-for-version>
                <constant>false</constant>
                <evict-on-update>false</evict-on-update>
                <cache-limited>false</cache-limited>
                <subscribe>true</subscribe>
                <pre-load-enabled>true</pre-load-enabled>
                <pre-load-fetch-size>0</pre-load-fetch-size>
                <pre-load-handles>true</pre-load-handles>
                <domain-object>
                    <cache-limited>true</cache-limited>
                    <evict-on-update>true</evict-on-update>
                    <mode>cache</mode>
                    <uri>/Events/CalculationTrigger</uri>
                    <backing-store>
                        <enabled>false</enabled>
                        <table-name/>
                        <properties>
                            <property>
                                <name>ticker</name>
                            </property>
                            <property>
                                <name>price</name>
                            </property>
                            <property>
                                <name>volume</name>
                            </property>
                        </properties>
                    </backing-store>
                    <indexes>
                        <index/>
                    </indexes>
                </domain-object>
            </domain-objects>
        </cache-manager>
        <db-concepts>
            <db-uris/>
            <check-interval>60</check-interval>
            <inactivity-timeout>0</inactivity-timeout>
            <initial-size>5</initial-size>
            <max-size>5</max-size>
            <min-size>5</min-size>
            <property-check-interval>900</property-check-interval>
            <retry-count>-1</retry-count>
            <wait-timeout>1</wait-timeout>
        </db-concepts>
    </object-management>
    <rulesets>
        <rules id="all-rules">
            <uri>/</uri>
        </rules>
    </rulesets>
    <function-groups>
        <functions id="all-functions">
            <uri>/</uri>
        </functions>
    </function-groups>
    <destination-groups>
    </destination-groups>
    <log-configs>
        <log-config id="logConfig">
            <enabled>true</enabled>
            <roles>*:info</roles>
            <files>
                <enabled>true</enabled>
                <dir>logs</dir>
                <name/>
                <max-number>10</max-number>
                <max-size>5000000</max-size>
                <append>false</append>
            </files>
            <terminal>
                <enabled/>
                <sys-out-redirect/>
                <sys-err-redirect/>
                <encoding/>
            </terminal>
            <line-layout>
                <enabled>false</enabled>
                <class/>
                <arg/>
            </line-layout>
        </log-config>
    </log-configs>
    <agent-classes>
        <inference-agent-class id="inference-agent">
            <rules>
                <ref>all-rules</ref>
            </rules>
            <destinations>
            </destinations>
            <startup>
            	<uri>/RuleFunctions/StartUp</uri>
            </startup>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size/>
                    <max-time/>
                </eviction>
            </local-cache>
            <shared-queue>
                <size>1024</size>
                <workers>10</workers>
            </shared-queue>
            <load>
                <max-active/>
            </load>
            <concurrent-rtc>false</concurrent-rtc>
            <check-for-duplicates>false</check-for-duplicates>
            <businessworks>
                <uri/>
            </businessworks>
            <property-group>
                <!--
                <property name="be.engine.metric.lock.timeout" type="long" value="0"/>
                <property name="be.engine.metric.lock.retrycount" type="int" value="1"/>
                <property name="be.engine.metric.lock.islocal" type="boolean" value="true"/>
                -->
            </property-group>
        </inference-agent-class>
        <dashboard-agent-class id="dashboard-agent">
            <rules/>
            <destinations/>
            <startup/>
            <shutdown/>
            <property-group>
                <property name="java.security.auth.login.config" type="string" value="Q:/be/branches/5.1/runtime/modules/dashboard/config/jaas-config.config"/>
                <property name="be.auth.type" type="string" value="file"/>
                <property name="be.auth.file.location" type="string" value="Q:/be/branches/5.1/examples/views/TickerTracker/config/users.pwd"/>
                <property name="be.agent.dashboard.mal.storage.file.root" type="string" value="Q:/be/branches/5.1/runtime/modules/dashboard/agent/test/personalizeddata"/>
                <property name="be.agent.dashboard.*.force.stacktrace" type="boolean" value="true"/>
                <property name="be.agent.dashboard.hostname" type="string" value="localhost"/>
                <property name="be.agent.dashboard.mal.missing.original.element.loading.policy" type="string" value="drop"/>
        		<property name="be.agent.dashboard.plugin.beviews.cache.query.enforce" value="false"/>
        		<property name="be.agent.dashboard.plugin.beviews.drilldown.table.page.count" type="integer" value="5"/>
        		<property name="be.agent.dashboard.plugin.beviews.decimalformat" value="##################.#########"/>
            </property-group>
        </dashboard-agent-class>
        <query-agent-class id="query-agent">
            <destinations/>
            <startup/>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size>1024</max-size>
                    <max-time>900</max-time>
                </eviction>
            </local-cache>
            <shared-queue>
                <size>1024</size>
                <workers>10</workers>
            </shared-queue>
            <load>
                <max-active/>
            </load>
            <property-group/>
        </query-agent-class>
    </agent-classes>
    <processing-units>
        <processing-unit id="default">
            <agents>
                <agent>
                    <ref>inference-agent</ref>
                    <key>infagent</key>
                    <priority/>
                </agent>
                <agent>
                    <ref>dashboard-agent</ref>
                    <key>dashboardagent</key>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="dashboard">
            <agents>
                <agent>
                    <ref>dashboard-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>false</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="inference">
            <agents>
                <agent>
                    <ref>inference-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="bqlconsole">
            <agents>
                <agent>
                    <ref>query-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <db-concepts>false</db-concepts>
            <property-group>
            	<property name="be.agent.query.console" value="query-agent"/>
            </property-group>
        </processing-unit>
    </processing-units>
    <property-group>
    	<property-group name="auth">
            <property name="be.mm.auth.type" value="file"/>
            <property name="be.mm.auth.file.location" value="Q:/be/branches/5.1/runtime/mm/em/config/users.pwd"/>
        </property-group>
        <property-group name="tibas">
        	<property name="be.engine.cluster.as.discover.url" value="tibpgm://9999/"/>
        </property-group>
        <property-group comment="" name="database">
            <property name="tibco.clientVar.DB_URL" value="jdbc:oracle:thin:@localhost:1521:BE"/>
            <property name="tibco.clientVar.DB_USERNAME" value="tt_user"/>
            <property name="tibco.clientVar.DB_PASSWORD" value="tt_user"/>
        </property-group>
    </property-group>
</cluster>
