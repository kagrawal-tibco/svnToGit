<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cluster xmlns="http://tibco.com/businessevents/configuration/5.1">
    <revision>
        <version>24</version>
        <author>TIBCO Software Inc.</author>
        <date>2011/04/11 16:25:10</date>
        <comment/>
    </revision>
    <name>fdd-anpatil-dt</name>
    <message-encoding>UTF-8</message-encoding>
    <object-management>
        <memory-manager/>
        <db-concepts>
            <db-uris/>
            <check-interval>60</check-interval>
            <inactivity-timeout>0</inactivity-timeout>
            <initial-size>5</initial-size>
            <max-size>5</max-size>
            <min-size>5</min-size>
            <property-check-interval>900</property-check-interval>
            <retry-count>-1</retry-count>
            <wait-timeout>1</wait-timeout>
        </db-concepts>
    </object-management>
    <rulesets>
        <rules id="all-rules">
            <uri>/</uri>
        </rules>
    </rulesets>
    <function-groups>
        <functions id="all-functions">
            <uri>/</uri>
        </functions>
    </function-groups>
    <destination-groups>
        <destinations id="Http">
            <destination id="AllOperations">
                <pre-processor>/RuleFunctions/PreProcessor</pre-processor>
                <queue-size>0</queue-size>
                <thread-count />
                <threading-model>caller</threading-model>
                <uri>/Channels/HTTP/AllOps</uri>
            </destination>
        </destinations>
    </destination-groups>
    <log-configs>
        <log-config id="logConfig">
            <enabled>true</enabled>
            <roles>*:info runtime.perf:info runtime.session:trace</roles>
            <files>
                <enabled>true</enabled>
                <dir>logs</dir>
                <name/>
                <max-number>10</max-number>
                <max-size>5000000</max-size>
                <append>false</append>
            </files>
            <terminal>
                <enabled/>
                <sys-out-redirect/>
                <sys-err-redirect/>
                <encoding/>
            </terminal>
            <line-layout>
                <enabled>false</enabled>
                <class/>
                <arg/>
            </line-layout>
        </log-config>
    </log-configs>
    <agent-classes>
        <dashboard-agent-class id="dashboard-agent">
            <rules/>
            <destinations/>
            <startup/>
            <shutdown/>
            <property-group>
                <!-- property name="Agent.dashboard-agent.recoverOnStartup" type="boolean" value="false"/-->
                <property name="java.security.auth.login.config" type="string" value="Q:/be/branches/5.1/runtime/modules/dashboard/config/jaas-config.config"/>
                <property name="be.auth.type" type="string" value="file"/>
                <property name="be.auth.file.location" type="string" value="Q:/be/branches/5.1/examples/views/FraudDetectionDashboard/config/users.pwd"/>
                <property name="be.agent.dashboard.mal.storage.file.root" type="string" value="Q:/be/branches/5.1/runtime/modules/dashboard/agent/test/personalizeddata"/>
                <property name="be.agent.dashboard.plugin.beviews.demomode" type="boolean" value="false"/>
                <property name="be.agent.dashboard.plugin.beviews.demomode.update.frequency" type="long" value="3600000"/>
                <property name="be.agent.dashboard.timeformats" type="string" value="hh:mm"/>
                <!-- <property name="be.agent.dashboard.rollover.time" type="string" value="12:12"/> -->
                <property name="be.agent.dashboard.rollover.policy" type="string" value="purge"/>
                <property name="be.agent.dashboard.*.force.stacktrace" type="boolean" value="true"/>
                <!--property name="be.agent.dashboard.plugin.beviews.updates.buffer.capacity" type="integer" value="2"/-->
                <property name="be.agent.dashboard.hostname" type="string" value="localhost"/>
                <property name="be.agent.dashboard.session.timeout" type="long" value="900000"/>
                <property name="be.agent.dashboard.plugin.beviews.drilldown.table.page.count" type="integer" value="5"/>
                <property name="be.agent.dashboard.http.maxprocessors" value="100"/>
                <property name="be.agent.dashboard.http.acceptcount" value="200"/>
                <property name="be.engine.jmx.connector.port" value="8888"/>
                <property name="be.agent.dashboard.legacy.search.page.enabled" value="false"/>
                <property name="be.agent.dashboard.plugin.beviews.drilldown.sources.aggregate.visible.columntypes" value="all"/>
                <property name="be.agent.dashboard.plugin.beviews.drilldown.include.dependent.systemfields" value="true"/>
                <property name="be.agent.dashboard.plugin.beviews.cache.query.enforce" value="false"/>
                <property name="be.agent.dashboard.plugin.beviews.cache.query.bindings.enabled" value="false"/>
                <!-- property name="be.agent.dashboard.plugin.beviews.filter.editor.contentassist.generation.enabled" value="true" /-->
            </property-group>
        </dashboard-agent-class>
        <inference-agent-class id="inference-agent">
            <rules>
                <ref>all-rules</ref>
            </rules>
            <destinations>
                <ref>Http</ref>
            </destinations>
            <startup/>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size/>
                    <max-time/>
                </eviction>
            </local-cache>
            <shared-queue>
                <size>1024</size>
                <workers>10</workers>
            </shared-queue>
            <load>
                <max-active/>
            </load>
            <concurrent-rtc>false</concurrent-rtc>
            <check-for-duplicates>false</check-for-duplicates>
            <businessworks>
                <uri/>
            </businessworks>
            <property-group>
                <!--
                <property name="be.engine.metric.lock.timeout" type="long" value="0"/>
                <property name="be.engine.metric.lock.retrycount" type="int" value="1"/>
                <property name="be.engine.metric.lock.islocal" type="boolean" value="true"/>
                -->
            </property-group>
        </inference-agent-class>
        <query-agent-class id="query-agent">
            <destinations/>
            <startup/>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size>1024</max-size>
                    <max-time>900</max-time>
                </eviction>
            </local-cache>
            <shared-queue>
                <size>1024</size>
                <workers>10</workers>
            </shared-queue>
            <load>
                <max-active/>
            </load>
            <property-group/>
        </query-agent-class>
    </agent-classes>
    <processing-units>
        <processing-unit id="dashboardagent">
            <agents>
                <agent>
                    <ref>dashboard-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>false</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="inferenceagent">
            <agents>
                <agent>
                    <ref>inference-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="default">
            <agents>
                <agent>
                    <ref>inference-agent</ref>
                    <key>infagent</key>
                    <priority/>
                </agent>
                <agent>
                    <ref>dashboard-agent</ref>
                    <key>dashboardagent</key>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="bqlconsole">
            <agents>
                <agent>
                    <ref>query-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group>
                <property name="be.agent.query.console" value="query-agent"/>
            </property-group>
        </processing-unit>
    </processing-units>
    <property-group>
        <property-group name="auth">
            <property name="be.mm.auth.type" value="file"/>
            <property name="be.mm.auth.file.location" value="Q:/be/branches/runtime/5.1/mm/em/config/users.pwd"/>
        </property-group>
        <property-group name="tibas">
            <property name="be.engine.cluster.as.discover.url" value="tibpgm://9999/"/>
            <property name="be.engine.cluster.as.tuple.explicit" value="true"/>
        </property-group>
        <property-group comment="" name="database">
            <property name="tibco.clientVar.DB_URL" value="jdbc:oracle:thin:@localhost:1521:BE"/>
            <property name="tibco.clientVar.DB_USERNAME" value="fdd_user"/>
            <property name="tibco.clientVar.DB_PASSWORD" value="fdd_user"/>
        </property-group>
        <property-group name="http perf">
        </property-group>
        <property-group>
        	<property name="be.stats.threading.model" value="single"/>
        </property-group>
    </property-group>
</cluster>
