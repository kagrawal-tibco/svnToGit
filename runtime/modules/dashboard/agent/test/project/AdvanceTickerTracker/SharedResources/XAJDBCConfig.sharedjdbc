<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BWSharedResource>
    <name>XAJDBCConfig</name>
    <resourceType>ae.shared.JDBCSharedResource</resourceType>
    <description/>
    <config>
        <description/>
        <resourceType>ae.shared.JDBCSharedResource</resourceType>
        <driver>oracle.jdbc.OracleDriver</driver>
        <maxConnections>5</maxConnections>
        <loginTimeout>0</loginTimeout>
        <connectionType>XA</connectionType>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
        <location>jdbc:oracle:thin:@&lt;host&gt;:&lt;port#&gt;:&lt;db_instancename&gt;</location>
        <user>%%DB_USERNAME%%</user>
        <password>%%DB_PASSWORD%%</password>
        <ContextFactory>com.tibco.tibjms.naming.TibjmsInitialContextFactory</ContextFactory>
        <ProviderUrl>tibjmsnaming://localhost:7222</ProviderUrl>
        <UserName/>
        <UserPassword>#!5c6wtz747v10zZ5Xs4UE3A==</UserPassword>
        <jndiDataSourceName>ddddd</jndiDataSourceName>
        <JndiSharedConfiguration/>
        <xaDsClass>oracle.jdbc.xa.client.OracleXADataSource</xaDsClass>
    </config>
</BWSharedResource>
