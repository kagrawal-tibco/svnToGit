<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cluster xmlns="http://tibco.com/businessevents/configuration/5.1">
    <revision>
        <version>45</version>
        <author>mwiley</author>
        <date>2012/05/16 15:25:40</date>
        <comment/>
    </revision>
    <name>BEViewsTestingDB</name>
    <message-encoding>UTF-8</message-encoding>
    <object-management>
        <cache-manager>
            <provider>
                <name>TIBCO</name>
            </provider>
            <cache-agent-quorum/>
            <backup-copies>1</backup-copies>
            <entity-cache-size>10000</entity-cache-size>
            <object-table>
                <max-size>100000</max-size>
            </object-table>
            <backing-store>
                <persistence-option>None</persistence-option>
                <cache-aside>true</cache-aside>
                <cache-loader-class/>
                <enforce-pools>true</enforce-pools>
                <strategy>jdbc</strategy>
                <type>Oracle</type>
                <data-store-path/>
                <persistence-policy>ASYNC</persistence-policy>
                <primary-connection>
                    <uri>/SharedResources/JDBCConfig.sharedjdbc</uri>
                    <initial-size>4</initial-size>
                    <max-size>5</max-size>
                    <min-size>1</min-size>
                </primary-connection>
            </backing-store>
            <domain-objects>
                <default-mode>cache</default-mode>
                <check-for-version>true</check-for-version>
                <constant>false</constant>
                <enable-tracking>true</enable-tracking>
                <evict-on-update>false</evict-on-update>
                <cache-limited>false</cache-limited>
                <subscribe>true</subscribe>
                <pre-load-enabled>true</pre-load-enabled>
                <pre-load-fetch-size>0</pre-load-fetch-size>
                <pre-load-handles>true</pre-load-handles>
            </domain-objects>
        </cache-manager>
        <db-concepts>
            <db-uris/>
            <check-interval>60</check-interval>
            <inactivity-timeout>0</inactivity-timeout>
            <initial-size>5</initial-size>
            <max-size>5</max-size>
            <min-size>5</min-size>
            <property-check-interval>900</property-check-interval>
            <retry-count>-1</retry-count>
            <wait-timeout>1</wait-timeout>
        </db-concepts>
    </object-management>
    <rulesets>
        <rules id="all-rules">
            <uri>/</uri>
        </rules>
    </rulesets>
    <function-groups>
        <functions id="all-functions">
            <uri>/</uri>
        </functions>
    </function-groups>
    <destination-groups>
        <destinations id="HTTP">
            <destination id="chart">
                <pre-processor>/RuleFunctions/PreProcessor</pre-processor>
                <queue-size>0</queue-size>
                <thread-count/>
                <threading-model>caller</threading-model>
                <uri>/Channels/HTTP/chart</uri>
            </destination>
            <destination id="stateMachine">
                <pre-processor>/RuleFunctions/PreProcessor</pre-processor>
                <queue-size>0</queue-size>
                <thread-count/>
                <threading-model>caller</threading-model>
                <uri>/Channels/HTTP/stateMachine</uri>
            </destination>
            <destination id="table">
                <pre-processor>/RuleFunctions/PreProcessor</pre-processor>
                <queue-size>0</queue-size>
                <thread-count/>
                <threading-model>caller</threading-model>
                <uri>/Channels/HTTP/table</uri>
            </destination>
        </destinations>
    </destination-groups>
    <process-groups/>
    <log-configs>
        <log-config id="logConfig">
            <enabled>true</enabled>
            <roles>*:info</roles>
            <files>
                <enabled>true</enabled>
                <dir>logs</dir>
                <name/>
                <max-number>10</max-number>
                <max-size>5000000</max-size>
                <append>false</append>
            </files>
            <terminal>
                <enabled/>
                <sys-out-redirect/>
                <sys-err-redirect/>
                <encoding/>
            </terminal>
            <line-layout>
                <enabled>false</enabled>
                <class/>
                <arg/>
            </line-layout>
        </log-config>
    </log-configs>
    <agent-classes>
        <cache-agent-class id="cache-class">
            <property-group/>
        </cache-agent-class>
        <dashboard-agent-class id="dashboard-class">
            <rules>
                <uri>/</uri>
            </rules>
            <destinations/>
            <startup/>
            <shutdown/>
            <property-group>
                <property name="be.auth.type" type="string" value="file"/>
                <property name="be.auth.file.location" value="Q:/be/branches/5.1/runtime/modules/dashboard/agent/test/project/BEViewsTesting/users.pwd"/>
                <property name="be.agent.dashboard.pullrequestport" value="8181"/>
                <property name="be.agent.dashboard.plugin.beviews.cache.query.enabled" value="true"/>
                <property name="be.agent.dashboard.mal.storage.file.root" value="Q:/be/branches/5.1/runtime/modules/dashboard/agent/test/personalizeddata"/>
                <property name="be.agent.dashboard.*.force.stacktrace" value="true"/>
            </property-group>
        </dashboard-agent-class>
        <inference-agent-class id="inference-agent">
            <rules>
                <ref>all-rules</ref>
            </rules>
            <destinations>
                <ref>HTTP</ref>
            </destinations>
            <startup/>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size/>
                    <max-time/>
                </eviction>
            </local-cache>
            <shared-queue>
                <size>1024</size>
                <workers>10</workers>
            </shared-queue>
            <load>
                <max-active/>
            </load>
            <concurrent-rtc>false</concurrent-rtc>
            <check-for-duplicates>false</check-for-duplicates>
            <businessworks>
                <uri/>
            </businessworks>
            <property-group/>
        </inference-agent-class>
    </agent-classes>
    <processing-units>
        <processing-unit id="dashboardagent">
            <agents>
                <agent>
                    <ref>dashboard-class</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="inferenceagent">
            <agents>
                <agent>
                    <ref>inference-agent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>true</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
        <processing-unit id="default">
            <agents>
                <agent>
                    <ref>inference-agent</ref>
                    <key>infagent</key>
                    <priority/>
                </agent>
                <agent>
                    <ref>dashboard-class</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <cache-storage-enabled>false</cache-storage-enabled>
            <db-concepts>false</db-concepts>
            <property-group/>
        </processing-unit>
    </processing-units>
    <load-balancer-configs>
        <pair-configs/>
        <adhoc-configs/>
    </load-balancer-configs>
    <property-group>
        <property-group comment="" name="auth">
            <property name="be.mm.auth.type" value="file"/>
            <property name="be.mm.auth.file.location" value="Q:/be/branches/runtime/5.1/mm/em/config/users.pwd"/>
        </property-group>
        <property-group comment="" name="tibas">
            <property name="be.engine.cluster.as.discover.url" value="tibpgm://9999/"/>
        </property-group>
        <property-group comment="" name="database">
            <property name="tibco.clientVar.DB_URL" value="jdbc:oracle:thin:@localhost:1521:XE"/>
            <property name="tibco.clientVar.DB_USERNAME" value="be_user"/>
            <property name="tibco.clientVar.DB_PASSWORD" value="be_user"/>
        </property-group>
        <property-group comment="" name="http perf"/>
    </property-group>
</cluster>
