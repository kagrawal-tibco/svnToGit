<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ogl="http://tibco.com/businessevents/views/ogl" targetNamespace="http://tibco.com/businessevents/views/ogl" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<simpleType name="BooleanOptions">
		<annotation>
			<documentation>Represents the options for a boolean value [HACK since Castor fails to validate a false value on a attribute]</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="true"/>
			<enumeration value="false"/>
		</restriction>
	</simpleType>
	<simpleType name="DisplayMode">
		<annotation>
			<documentation>Represents the enumeration for the display mode for the pages</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="inplace"/>
			<enumeration value="modaldialog"/>
			<enumeration value="nonmodaldialog"/>
		</restriction>
	</simpleType>
	<simpleType name="PartitionState">
		<annotation>
			<documentation>Represents the enumeration for the partition states in a page</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="open"/>
			<enumeration value="closed"/>
		</restriction>
	</simpleType>
	<simpleType name="ScrollBarConfig">
		<annotation>
			<documentation>Represents the enumeration for the scroll bar configuration in a panel</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="auto"/>
			<enumeration value="never"/>
			<enumeration value="yes"/>
		</restriction>
	</simpleType>
	<simpleType name="PanelState">
		<annotation>
			<documentation>Represents the enumeration for the panel states in a page</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="maximized"/>
			<enumeration value="minimized"/>
			<enumeration value="normal"/>
		</restriction>
	</simpleType>
	<simpleType name="HexColor">
		<annotation>
			<documentation>Represents the color in hexadecimal</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[0-9a-fA-F]{6}"/>
		</restriction>
	</simpleType>
	<simpleType name="GradientDirection">
		<annotation>
			<documentation>Represents the enumeration for gradient directions</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="toptobottom"/>
			<enumeration value="lefttoright"/>
			<enumeration value="bottomtotop"/>
			<enumeration value="righttoleft"/>
		</restriction>
	</simpleType>
	<simpleType name="FontStyle">
		<annotation>
			<documentation>Represents the enumeration for font style</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="normal"/>
			<enumeration value="italic"/>
			<enumeration value="bold"/>
			<enumeration value="bold,italic"/>
		</restriction>
	</simpleType>
	<simpleType name="ColumnType">
		<annotation>
			<documentation>Represents the enumeration for cell types</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="text"/>
			<enumeration value="indicator"/>
			<enumeration value="progress"/>
		</restriction>
	</simpleType>
	<simpleType name="ColumnAlignment">
		<annotation>
			<documentation>Represents the enumeration for alignments within a cell </documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="left"/>
			<enumeration value="center"/>
			<enumeration value="right"/>
		</restriction>
	</simpleType>
	<simpleType name="LegendOrientation">
		<annotation>
			<documentation>Represents the enumeration for legend orientation</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="vertical"/>
			<enumeration value="horizontal"/>
		</restriction>
	</simpleType>
	<simpleType name="LegendAnchor">
		<annotation>
			<documentation>Represents the enumeration for legend anchor</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="north"/>
			<enumeration value="east"/>
			<enumeration value="west"/>
			<enumeration value="south"/>
		</restriction>
	</simpleType>
	<simpleType name="LineStyle">
		<annotation>
			<documentation>Represents the enumeration for styles of lines in the plot area background</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="none"/>
			<enumeration value="vertical"/>
			<enumeration value="horizontal"/>
			<enumeration value="both"/>
		</restriction>
	</simpleType>
	<simpleType name="Quadrant">
		<annotation>
			<documentation>Represents the enumeration for series anchor as in which quadrant should the series be rendered</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="Q1"/>
			<enumeration value="Q2"/>
			<enumeration value="Q3"/>
			<enumeration value="Q4"/>
		</restriction>
	</simpleType>
	<simpleType name="Scale">
		<annotation>
			<documentation>Represents the enumeration for scale that can be used on the value axis</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="normal"/>
			<enumeration value="none"/>
			<enumeration value="thousands"/>
			<enumeration value="millions"/>
			<enumeration value="billions"/>
			<enumeration value="trillions"/>
			<enumeration value="quadrillions"/>
			<enumeration value="logarithmic"/>
		</restriction>
	</simpleType>
	<simpleType name="AxisPositioning">
		<annotation>
			<documentation>Represents the enumeration for positioning for a axis</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="left"/>
			<enumeration value="above"/>
			<enumeration value="right"/>
			<enumeration value="below"/>
		</restriction>
	</simpleType>
	<simpleType name="ChartRenderingType">
		<annotation>
			<documentation>Represents the enumeration for types of rendering for a series</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="verticalbar"/>
			<enumeration value="horizontalbar"/>
			<enumeration value="pie"/>
			<enumeration value="line"/>
			<enumeration value="area"/>
			<enumeration value="verticalrange"/>
			<enumeration value="horizontalrange"/>
			<enumeration value="scatter"/>
		</restriction>
	</simpleType>
	<simpleType name="PlacementStyle">
		<annotation>
			<documentation>Represents the enumeration for category axis labels placement</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="automatic"/>
			<enumeration value="continous"/>
			<enumeration value="staggered"/>
			<enumeration value="skipped"/>
		</restriction>
	</simpleType>
	<simpleType name="CommandType">
		<annotation>
			<documentation>The type of action</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="launchinternallink">
				<annotation>
					<documentation>Indicates a action which launches a query (using the search/drilldown interface)</documentation>
				</annotation>
			</enumeration>
			<enumeration value="launchcomponent">
				<annotation>
					<documentation>Indicates a action which launches a visualization (using a visualization holding panel)</documentation>
				</annotation>
			</enumeration>
			<enumeration value="launchexternallink"/>
			<enumeration value="showpanel"/>
			<enumeration value="hidepanel"/>
			<enumeration value="showdialog"/>
			<enumeration value="hidedialog"/>
			<enumeration value="delete"/>
			<enumeration value="save"/>
			<enumeration value="recurring"/>
			<enumeration value="userdefined"/>
		</restriction>
	</simpleType>
	<simpleType name="RowConfigType">
		<annotation>
			<documentation>Represents the enumeration for the row config types</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="header"/>
			<enumeration value="data"/>
		</restriction>
	</simpleType>
	<simpleType name="GroupConfigRoleType">
		<annotation>
			<documentation>Represents the enumeration for the Group Series config role types</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="trendlow"/>
			<enumeration value="trendhigh"/>
			<enumeration value="trendmedian"/>
			<enumeration value="simple"/>
		</restriction>
	</simpleType>
	<simpleType name="DataType">
		<annotation>
			<documentation>Represents the general purpose enumeration for datatype</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="datetime"/>
			<enumeration value="string"/>
			<enumeration value="boolean"/>
			<enumeration value="double"/>
			<enumeration value="float"/>
			<enumeration value="int"/>
			<enumeration value="long"/>
			<enumeration value="short"/>
		</restriction>
	</simpleType>
	<simpleType name="PageSetLaunchType">
		<annotation>
			<documentation>Represents the enumeration for launch type for page set in pagesetrowconfig</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="internal"/>
			<enumeration value="external"/>
		</restriction>
	</simpleType>
	<simpleType name="VariableType">
		<annotation>
			<documentation>Represents the type of the page variable</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="normal"/>
			<enumeration value="hidden"/>
		</restriction>
	</simpleType>
	<complexType name="CoordinatePointType">
		<attribute name="x" type="nonNegativeInteger" use="required"/>
		<attribute name="y" type="nonNegativeInteger" use="required"/>
		<attribute name="label" type="string" use="optional"/>
		<attribute name="icon" use="optional">
			<simpleType>
				<restriction base="string">
					<enumeration value="arrow"/>
					<enumeration value="filled-arrow"/>
					<enumeration value="circle"/>
					<enumeration value="box"/>
				</restriction>
			</simpleType>
		</attribute>
	</complexType>
	<complexType name="RectangleSizeType">
		<attribute name="width" type="nonNegativeInteger" use="required"/>
		<attribute name="height" type="nonNegativeInteger" use="required"/>
	</complexType>
	<complexType name="Rectangle">
		<annotation>
			<documentation>Specifies a rectangle object or the pixel cordinates and size for an object. This differs from ComponentSizeType in that its units are pixels and values are wholeNumber-enforced.</documentation>
		</annotation>
		<sequence>
			<element name="position" type="ogl:CoordinatePointType"/>
			<element name="size" type="ogl:RectangleSizeType"/>
		</sequence>
	</complexType>
	<complexType name="TypeSpecificAttributeType">
		<annotation>
			<documentation>Represents any non standard values that need to be passed a object to properly configure it. e.g. highlightcolor, threshold, punchout</documentation>
		</annotation>
		<simpleContent>
			<extension base="string">
				<attribute name="name" type="string" use="required">
					<annotation>
						<documentation>The name of the hint</documentation>
					</annotation>
				</attribute>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ColorConfigType">
		<annotation>
			<documentation>Represents the specification for the color properties</documentation>
		</annotation>
		<attribute name="background" type="ogl:HexColor" use="required"/>
		<attribute name="rollover" type="ogl:HexColor" use="required"/>
		<attribute name="press" type="ogl:HexColor" use="optional"/>
		<attribute name="selected_rollover" type="ogl:HexColor" use="optional"/>
		<attribute name="selected_press" type="ogl:HexColor" use="optional"/>
	</complexType>
	<complexType name="TickLabelConfigType">
		<annotation>
			<documentation>Represents the label styling for a tick </documentation>
		</annotation>
		<attribute name="fontsize" type="int" use="optional"/>
		<attribute name="fontcolor" type="ogl:HexColor" use="optional" default="000000"/>
		<attribute name="fontstyle" type="ogl:FontStyle" use="optional" default="normal"/>
		<attribute name="placement" type="ogl:PlacementStyle" use="optional" default="continous"/>
		<attribute name="rotation" type="int" use="optional" default="0">
			<annotation>
				<documentation>Indicates how the text should be rotated in degrees. Positive value means rotating clockwise. Negative value means rotating anticlockwise. </documentation>
			</annotation>
		</attribute>
		<attribute name="skipfactor" type="int" use="optional" default="1"/>
	</complexType>
	<complexType name="CategoryAxisConfigType">
		<annotation>
			<documentation>Represents the configuration for a category axis</documentation>
		</annotation>
		<sequence>
			<element name="ticklabelconfig" type="ogl:TickLabelConfigType" minOccurs="0"/>
		</sequence>
		<attribute name="name" type="string" use="optional"/>
		<attribute name="datatype" type="ogl:DataType" use="optional"/>
		<attribute name="fontsize" type="int" use="optional"/>
		<attribute name="fontcolor" type="ogl:HexColor" use="optional" default="000000"/>
		<attribute name="fontstyle" type="ogl:FontStyle" use="optional" default="normal"/>
		<attribute name="relativeposition" type="ogl:AxisPositioning" use="optional"/>
	</complexType>
	<complexType name="ValueAxisConfigType">
		<annotation>
			<documentation>Represents the configuration for a value axis</documentation>
		</annotation>
		<complexContent>
			<extension base="ogl:CategoryAxisConfigType">
				<attribute name="scale" type="ogl:Scale" use="optional" default="normal"/>
				<attribute name="noofvisualdivisions" type="int" use="required"/>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ParameterType">
		<annotation>
			<documentation>Represents the parameter in the actionconfig</documentation>
		</annotation>
		<simpleContent>
			<extension base="string">
				<attribute name="name" type="string" use="required"/>
			</extension>
		</simpleContent>
	</complexType>
	<element name="configparam" type="ogl:ParameterType">
		<annotation>
			<documentation>Represents a configuration parameter for a action config</documentation>
		</annotation>
	</element>
	<element name="param" type="ogl:ParameterType">
		<annotation>
			<documentation>Represents a hard coded parameter for a action config</documentation>
		</annotation>
	</element>
	<element name="dynamicparam" type="ogl:ParameterType">
		<annotation>
			<documentation>Represents a dynamic parameter for a action config. Syntax is 'someobjectname.someobjectproperty' 
				currentpageset - represents the currently viewed pageset
				currentpage - represents the currently viewed page in the dashboard 
				currentpanel - represents the currently focused panel in the dashboard 
				currentcomponent - represents the currently focused component 'the componentconfig' tag in the layout xml'
			    currentcomponentmodel - represents the currently focused component's config as in the one obtained via the getcomponentconfig.do API
			</documentation>
		</annotation>
	</element>
	<complexType name="ActionConfigType">
		<annotation>
			<documentation>Represents the configuration of a action to be executed in the front end UI</documentation>
		</annotation>
		<sequence>
			<element name="text" type="string"/>
			<element ref="ogl:configparam" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="ogl:param" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="ogl:dynamicparam" minOccurs="0" maxOccurs="unbounded"/>
			<element name="toggleactionconfig" minOccurs="0">
				<complexType>
					<sequence>
						<element name="text" type="string"/>
						<element ref="ogl:configparam" minOccurs="0" maxOccurs="unbounded"/>
						<element ref="ogl:param" minOccurs="0" maxOccurs="unbounded"/>
						<element ref="ogl:dynamicparam" minOccurs="0" maxOccurs="unbounded"/>
					</sequence>
					<attribute name="command" type="ogl:CommandType" use="required"/>
				</complexType>
			</element>
			<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attribute name="command" type="ogl:CommandType" use="optional"/>
		<attribute name="disabled" type="ogl:BooleanOptions" use="optional"/>
		<attribute name="defaultitem" type="ogl:BooleanOptions" use="optional"/>
		<attribute name="qualifier" type="string"/>
	</complexType>
	<complexType name="VisualizationModelType">
		<annotation>
			<documentation>Represents the base for a visualization model [contains config and data]</documentation>
		</annotation>
		<sequence>
			<element name="help" type="string" minOccurs="0">
				<annotation>
					<documentation>Represents the text shown as help against a component</documentation>
				</annotation>
			</element>
			<element name="description" type="string" minOccurs="0">
				<annotation>
					<documentation>Represents the text shown as description against a component</documentation>
				</annotation>
			</element>
			<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>Represents the menus shown against a component</documentation>
				</annotation>
			</element>
			<element ref="ogl:visualizationdata" minOccurs="0"/>
		</sequence>
		<attribute name="componentid" type="string" use="required"/>
		<attribute name="componentname" type="string" use="required"/>
		<attribute name="componenttype" type="string" use="required"/>
	</complexType>
	<complexType name="VisualizationConfigType">
		<annotation>
			<documentation>Represents the base for the configuration section of a visualization model [contains seriescontainer]</documentation>
		</annotation>
		<attribute name="backgroundcolor" type="ogl:HexColor" use="required"/>
		<attribute name="gradientendcolor" type="ogl:HexColor" use="optional"/>
		<attribute name="gradientdirection" type="ogl:GradientDirection" use="optional"/>
		<attribute name="componentid" type="string" use="required"/>
	</complexType>
	<complexType name="VisualizationSeriesContainerType">
		<annotation>
			<documentation>Represents the base for the series configuration holder in the configuration section</documentation>
		</annotation>
	</complexType>
	<complexType name="LayoutConstraints">
		<attribute name="rowspan" type="int" use="optional"/>
		<attribute name="colspan" type="int" use="optional"/>
	</complexType>
	<complexType name="ComponentSizeType">
		<annotation>
			<documentation>Represents the configuration size of a component (provided to component gallery)</documentation>
		</annotation>
		<complexContent>
			<extension base="ogl:LayoutConstraints">
				<attribute name="width"/>
				<attribute name="height"/>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ComponentDefinition">
		<annotation>
			<documentation>Represents a general purpose definition of a component (used in pagesconfig/partitionconfig/panelconfig and compgallery/category)</documentation>
		</annotation>
		<choice>
			<element name="layoutconstraints" type="ogl:LayoutConstraints" minOccurs="0"/>
			<element name="prefsize" type="ogl:ComponentSizeType"/>
		</choice>
		<attribute name="id" type="string" use="required"/>
		<attribute name="name" type="string" use="required"/>
		<attribute name="title" type="string" use="required"/>
		<attribute name="movieurl" type="string" use="optional"/>
		<attribute name="type" type="string" use="required"/>
		<attribute name="deletable" type="ogl:BooleanOptions" use="optional"/>
		<attribute name="disabled" type="ogl:BooleanOptions" use="optional"/>
	</complexType>
	<element name="visualizationdata">
		<annotation>
			<documentation>Represents the visualization data</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="datarow" minOccurs="0" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element name="tooltip" type="string" minOccurs="0">
								<annotation>
									<documentation>Represents the tooltip to be associated with the flag. Should be preferably inside a CDATA section</documentation>
								</annotation>
							</element>
							<element name="link" type="string" minOccurs="0">
								<annotation>
									<documentation>Represents the link to be associated with the flag. Should be preferably inside a CDATA section</documentation>
								</annotation>
							</element>
							<element name="datacolumn" maxOccurs="unbounded">
								<complexType>
									<sequence>
										<element name="displayvalue" type="string" minOccurs="0">
											<annotation>
												<documentation>Formatted Value to be displayed. $1000</documentation>
											</annotation>
										</element>
										<element name="value" type="string" minOccurs="0">
											<annotation>
												<documentation>Actual value. Chart</documentation>
											</annotation>
										</element>
										<element name="basecolor" type="string" minOccurs="0">
											<annotation>
												<documentation>This will be used by indicator and progress bar or chart or text background color</documentation>
											</annotation>
										</element>
										<element name="highlightcolor" type="string" minOccurs="0">
											<annotation>
												<documentation>This will be used by chart color</documentation>
											</annotation>
										</element>
										<element name="tooltip" type="string" minOccurs="0">
											<annotation>
												<documentation>Represents the tooltip to be associated with the flag. Should be preferably inside a CDATA section</documentation>
											</annotation>
										</element>
										<element name="link" type="string" minOccurs="0">
											<annotation>
												<documentation>Represents the link to be associated with the flag. Should be preferably inside a CDATA section</documentation>
											</annotation>
										</element>
										<element name="typespecificattribute" type="ogl:TypeSpecificAttributeType" minOccurs="0" maxOccurs="unbounded">
											<annotation>
												<documentation>Represents all the configuration attributed that will be needed for rendering a particular type properly. As of now only progressbar type would use two attributes viz. color and threshold</documentation>
											</annotation>
										</element>
										<element name="font" minOccurs="0">
											<complexType>
												<attribute name="color" type="ogl:HexColor" use="required"/>
												<attribute name="style" type="ogl:FontStyle" use="required"/>
											</complexType>
										</element>
										<element name="sortvalue" type="string" minOccurs="0"/>
									</sequence>
									<attribute name="id" type="string" use="required"/>
									<attribute name="templateid" type="string" use="optional"/>
								</complexType>
							</element>
						</sequence>
						<attribute name="id" type="string" use="required"/>
						<attribute name="templatetype" type="ogl:RowConfigType" use="required"/>
						<attribute name="visualtype" type="string" use="required"/>
						<attribute name="templateid" type="string" use="optional"/>
						<attribute name="sortorder" type="int" use="optional"/>
					</complexType>
				</element>
			</sequence>
			<attribute name="componentid" type="string" use="required"/>
		</complexType>
	</element>
	<element name="logincustomization">
		<annotation>
			<documentation>Represents the customization to be done to the login dialog</documentation>
		</annotation>
		<complexType>
			<attribute name="title" type="string" use="required">
				<annotation>
					<documentation>The text which is to be used as the title of the login dialog in Insight</documentation>
				</annotation>
			</attribute>
			<attribute name="imageurl" type="string" use="required">
				<annotation>
					<documentation>The url of the image which is to be used in the login dialog in Insight</documentation>
				</annotation>
			</attribute>
		</complexType>
	</element>
	<element name="hdrconfig">
		<annotation>
			<documentation>Represents the page header configuration</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="pagehdrconfig" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element name="title" type="string"/>
							<element name="image" minOccurs="0">
								<complexType>
									<attribute name="url" type="string" use="required"/>
									<attribute name="link" type="string" use="optional"/>
								</complexType>
							</element>
							<element name="hdrlinks" minOccurs="0" maxOccurs="unbounded">
								<annotation>
									<documentation>Represents the Header links</documentation>
								</annotation>
								<complexType>
									<attribute name="urlname" type="string" use="optional"/>
									<attribute name="urllink" type="string" use="required"/>
								</complexType>
							</element>
							<element name="hdractioncfg">
								<annotation>
									<documentation>Represents the menus shown against a component</documentation>
								</annotation>
								<complexType>
									<complexContent>
										<extension base="ogl:ActionConfigType"/>
									</complexContent>
								</complexType>
							</element>
						</sequence>
						<attribute name="pageid" type="string" use="required"/>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
	<element name="pagesconfig">
		<annotation>
			<documentation>Represents all the pages that a user will have access to when he/she logs in</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="pageconfig" maxOccurs="unbounded">
					<annotation>
						<documentation>Represents a single page definition</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="partitionconfig" maxOccurs="unbounded">
								<annotation>
									<documentation>The layout is divided into vertical parts and then each of these vertical parts is divided into horizontal parts. Vertical parts are referred to as "columns". Each column is dockable, meaning it can be closed or opened. The state defines the current dock state of the slide.  Each horizontal division is referred to as panels</documentation>
								</annotation>
								<complexType>
									<sequence>
										<element name="panelconfig" maxOccurs="unbounded">
											<annotation>
												<documentation>A panel forms the primary display item of the layout. Panels are placed one over the other in columnar fomat. The height of each panel is defined in the "height" attribute while the width is inherited from the slide's width</documentation>
											</annotation>
											<complexType>
												<sequence>
													<element name="layoutconfig" minOccurs="0">
														<complexType>
															<attribute name="type" type="string" use="required"/>
															<attribute name="width" type="int" use="optional"/>
															<attribute name="height" type="int" use="optional"/>
															<attribute name="movieurl" type="string" use="optional"/>
														</complexType>
													</element>
													<element name="componentconfig" type="ogl:ComponentDefinition" maxOccurs="unbounded"/>
												</sequence>
												<attribute name="span" type="string" use="required">
													<annotation>
														<documentation>Height of the panel. A numeric value is interpreted as number of pixels. A value ending with '%' is interpreted as a precentage value. and a value of * is interpreted as available height after heights for all previous panels has been computed</documentation>
													</annotation>
												</attribute>
												<attribute name="id" type="string" use="required">
													<annotation>
														<documentation>ID of the panel</documentation>
													</annotation>
												</attribute>
												<attribute name="scrollbars" type="ogl:ScrollBarConfig" use="optional" default="auto">
													<annotation>
														<documentation>Specifies whether scrollbars should be shown or not. it takes "auto", "none", "horizontal" and "vertical" as values</documentation>
													</annotation>
												</attribute>
												<attribute name="maximizable" type="ogl:BooleanOptions" use="optional" default="true">
													<annotation>
														<documentation>This specifies whether user can minimize maximize the panel</documentation>
													</annotation>
												</attribute>
												<attribute name="minimizable" type="ogl:BooleanOptions" use="optional" default="true"/>
												<attribute name="state" type="ogl:PanelState" use="optional" default="normal">
													<annotation>
														<documentation>The initial state of the panel</documentation>
													</annotation>
												</attribute>
												<attribute name="title" type="string" use="required"/>
												<attribute name="visible" type="ogl:BooleanOptions" use="optional" default="true"/>
												<attribute name="weightage" type="decimal" use="optional" default="0"/>
												<attribute name="supportsmenus" type="ogl:BooleanOptions" use="optional" default="false"/>
											</complexType>
										</element>
									</sequence>
									<attribute name="span" type="string" use="required">
										<annotation>
											<documentation>Width of the slide. A numeric value is interpreted as number of pixels. A value ending with '%' is interpreted as a precentage value. and a value of * is interpreted as available width after widths for all previous columns has been computed</documentation>
										</annotation>
									</attribute>
									<attribute name="state" type="ogl:PartitionState" use="optional" default="open">
										<annotation>
											<documentation>Docked state of the slide. It takes "open" and "closed" as values</documentation>
										</annotation>
									</attribute>
									<attribute name="dockable" type="ogl:BooleanOptions" use="optional" default="true">
										<annotation>
											<documentation>Specifies whether this slide can be docked at run-time</documentation>
										</annotation>
									</attribute>
									<attribute name="id" type="string" use="required">
										<annotation>
											<documentation>The id of the column</documentation>
										</annotation>
									</attribute>
								</complexType>
							</element>
							<element name="settings" minOccurs="0" maxOccurs="unbounded">
								<annotation>
									<documentation>This element holds the Visual as well as behavioural settings of the layout. Currently I am holding only the color scheme name</documentation>
								</annotation>
								<complexType>
									<simpleContent>
										<extension base="string">
											<attribute name="name" type="string" use="required"/>
										</extension>
									</simpleContent>
								</complexType>
							</element>
							<element name="variable" minOccurs="0" maxOccurs="unbounded">
								<annotation>
									<documentation>Represents page level variables. Variables can be either normal (visible to user) or hidden (no visible to user). Hidden variables needs to be passed to the dashboard agent in every request</documentation>
								</annotation>
								<complexType>
									<simpleContent>
										<extension base="string">
											<attribute name="name" type="string" use="required"/>
											<attribute name="datatype" type="ogl:DataType" use="optional" default="string"/>
											<attribute name="type" type="ogl:VariableType" use="optional" default="normal"/>
										</extension>
									</simpleContent>
								</complexType>
							</element>
							<element name="action" type="ogl:ActionConfigType" minOccurs="0" maxOccurs="unbounded">
								<annotation>
									<documentation>Represents any actions that need to be applied once the page has been loaded fully</documentation>
								</annotation>
							</element>
						</sequence>
						<attribute name="id" type="string" use="required">
							<annotation>
								<documentation>The id of the page. This value should be unique within the file scope.</documentation>
							</annotation>
						</attribute>
						<attribute name="name" type="string" use="required">
							<annotation>
								<documentation>The name of the page. This name appears on the tab.</documentation>
							</annotation>
						</attribute>
						<attribute name="displaymode" type="ogl:DisplayMode" use="required">
							<annotation>
								<documentation>Decides the rendering strategy for the pages. Depending on the specified strategy, Insight will show the page inplace or in a modal dialog or in a nonmodal dialog</documentation>
							</annotation>
						</attribute>
						<attribute name="type" type="string">
							<annotation>
								<documentation>The type of the page</documentation>
							</annotation>
						</attribute>
					</complexType>
				</element>
			</sequence>
			<attribute name="id" type="string" use="required"/>
			<attribute name="name" type="string" use="required"/>
			<attribute name="defaultpage" type="string" use="required">
				<annotation>
					<documentation>The id of the default page. This is the page which will be shown to the user when he/she logs in</documentation>
				</annotation>
			</attribute>
		</complexType>
	</element>
	<element name="columnconfig">
		<annotation>
			<documentation>Represents the configuration for columns in a rowconfig/ activityconfig/ pagesetselector</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="colorconfig" type="ogl:ColorConfigType" minOccurs="0"/>
				<element name="borderconfig" minOccurs="0">
					<complexType>
						<attribute name="bottom" type="string" use="optional"/>
						<attribute name="left" type="string" use="optional"/>
						<attribute name="right" type="string" use="optional"/>
						<attribute name="top" type="string" use="optional"/>
					</complexType>
				</element>
				<element name="typespecificattribute" type="ogl:TypeSpecificAttributeType" minOccurs="0" maxOccurs="unbounded">
					<annotation>
						<documentation>Represents all the configuration attributed that will be needed for rendering a particular type properly. As of now only progressbar type would use two attributes viz. color and threshold</documentation>
					</annotation>
				</element>
				<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0"/>
			</sequence>
			<attribute name="id" type="string" use="required"/>
			<attribute name="width" type="string"/>
			<attribute name="type" type="ogl:ColumnType" use="optional" default="text"/>
			<attribute name="align" type="ogl:ColumnAlignment" use="optional" default="center"/>
			<attribute name="fontsize" type="int" use="optional"/>
			<attribute name="fontcolor" type="ogl:HexColor" use="optional"/>
			<attribute name="fontstyle" type="ogl:FontStyle" use="optional" default="normal"/>
		</complexType>
	</element>
	<element name="textmodel">
		<annotation>
			<documentation>Represents the visualization model for text</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ogl:VisualizationModelType">
					<sequence>
						<element name="textconfig">
							<annotation>
								<documentation>Represents the configuration for the tabular text visualization</documentation>
							</annotation>
							<complexType>
								<complexContent>
									<extension base="ogl:VisualizationConfigType">
										<sequence>
											<element name="rowconfig" maxOccurs="unbounded">
												<annotation>
													<documentation>Represents the series container for text visualization </documentation>
												</annotation>
												<complexType>
													<complexContent>
														<extension base="ogl:VisualizationSeriesContainerType">
															<sequence>
																<element name="colorconfig" type="ogl:ColorConfigType" minOccurs="0"/>
																<element ref="ogl:columnconfig" maxOccurs="unbounded"/>
																<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0"/>
															</sequence>
															<attribute name="id" type="string" use="required"/>
															<attribute name="templatetype" type="ogl:RowConfigType" use="required"/>
															<attribute name="height" type="string" use="optional" default="17"/>
															<attribute name="visualtype" type="string" use="required"/>
															<attribute name="background" type="ogl:HexColor" use="optional"/>
															<attribute name="rollover" type="ogl:HexColor" use="optional"/>
															<attribute name="press" type="ogl:HexColor" use="optional"/>
															<attribute name="selected_rollover" type="ogl:HexColor" use="optional"/>
															<attribute name="selected_press" type="ogl:HexColor" use="optional"/>
														</extension>
													</complexContent>
												</complexType>
											</element>
										</sequence>
										<attribute name="maxrows" type="int" use="optional"/>
										<attribute name="pagesize" type="int"/>
									</extension>
								</complexContent>
							</complexType>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="chartmodel">
		<annotation>
			<documentation>Represents the visualization model for chart</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ogl:VisualizationModelType">
					<sequence>
						<element name="chartconfig">
							<annotation>
								<documentation>Represents the configuration for the chart visualization</documentation>
							</annotation>
							<complexType>
								<complexContent>
									<extension base="ogl:VisualizationConfigType">
										<sequence>
											<element name="legendconfig" minOccurs="0">
												<annotation>
													<documentation>Indicates legend configuration data. Absence indicates no legend to be shown.</documentation>
												</annotation>
												<complexType>
													<attribute name="orientation" type="ogl:LegendOrientation" use="required"/>
													<attribute name="anchor" type="ogl:LegendAnchor" use="required"/>
												</complexType>
											</element>
											<element name="plotareaconfig" minOccurs="0">
												<complexType>
													<sequence>
														<element name="plotpatternconfig" minOccurs="0">
															<annotation>
																<documentation>Decides the pattern to be used as background in the plot area. This is an optional element. Absence of this indicates plain plotting area background.</documentation>
															</annotation>
															<complexType>
																<attribute name="color" type="ogl:HexColor" use="required">
																	<annotation>
																		<documentation>The color to be used to plot the plotting pattern. This value is specified as in HTML e.g. #FFFFFF</documentation>
																	</annotation>
																</attribute>
																<attribute name="lines" type="ogl:LineStyle" use="optional" default="both"/>
															</complexType>
														</element>
													</sequence>
													<attribute name="backgroundcolor" type="ogl:HexColor" use="required"/>
													<attribute name="gradientendcolor" type="ogl:HexColor" use="optional"/>
													<attribute name="gradientdirection" type="ogl:GradientDirection" use="optional" default="toptobottom"/>
												</complexType>
											</element>
											<element name="categoryaxisconfig" type="ogl:CategoryAxisConfigType" minOccurs="0">
												<annotation>
													<documentation>Indicates all the specification needed for category. Absence indicates usage of default values.</documentation>
												</annotation>
											</element>
											<element name="valueaxisconfig" type="ogl:ValueAxisConfigType" minOccurs="0">
												<annotation>
													<documentation>Indicates all the specification needed for value. Absence indicates usage of default values.</documentation>
												</annotation>
											</element>
											<element name="charttypeconfig" maxOccurs="3">
												<complexType>
													<complexContent>
														<extension base="ogl:VisualizationSeriesContainerType">
															<sequence>
																<element name="seriesconfig" maxOccurs="unbounded">
																	<complexType>
																		<sequence>
																			<element name="tooltip" type="string" minOccurs="0">
																				<annotation>
																					<documentation>Represents the tooltip to be associated with the flag. Should be preferably inside a CDATA section</documentation>
																				</annotation>
																			</element>
																			<element name="link" type="string" minOccurs="0">
																				<annotation>
																					<documentation>Represents the link to be associated with the flag. Should be preferably inside a CDATA section</documentation>
																				</annotation>
																			</element>
																			<element name="valuelabelconfig" minOccurs="0">
																				<complexType>
																					<attribute name="fontsize" type="int" use="required"/>
																					<attribute name="fontcolor" type="ogl:HexColor" use="required"/>
																					<attribute name="fontstyle" type="ogl:FontStyle" use="optional" default="normal"/>
																					<!--<attribute name="anchor" type="ogl:LabelAnchor" use="required"/>-->
																				</complexType>
																			</element>
																			<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0"/>
																		</sequence>
																		<attribute name="name" type="string" use="required"/>
																		<attribute name="displayname" type="string" use="required"/>
																		<attribute name="anchor" type="ogl:Quadrant" use="optional" default="Q1"/>
																		<attribute name="basecolor" type="ogl:HexColor" use="required"/>
																		<attribute name="independentcategoryaxis" type="boolean" use="optional"/>
																		<attribute name="highlightcolor" type="ogl:HexColor" use="required"/>
																	</complexType>
																</element>
																<element name="valueaxisconfig" type="ogl:ValueAxisConfigType" minOccurs="0">
																	<annotation>
																		<documentation>Indicates all the specification needed for value. Absence indicates usage of default values.</documentation>
																	</annotation>
																</element>
																<element name="typespecificattribute" type="ogl:TypeSpecificAttributeType" minOccurs="0" maxOccurs="unbounded"/>
																<element name="seriesgroupconfig" minOccurs="0" maxOccurs="unbounded">
																	<complexType>
																		<sequence>
																			<element name="child" maxOccurs="unbounded">
																				<complexType>
																					<attribute name="seriesid" type="string" use="required"/>
																					<attribute name="role" type="ogl:GroupConfigRoleType" use="required"/>
																				</complexType>
																			</element>
																		</sequence>
																		<attribute name="id" type="string" use="required"/>
																		<attribute name="name" type="string" use="required"/>
																		<attribute name="masterseriescfgid" type="string" use="required"/>
																	</complexType>
																</element>
															</sequence>
															<attribute name="type" type="ogl:ChartRenderingType" use="required"/>
														</extension>
													</complexContent>
												</complexType>
											</element>
										</sequence>
									</extension>
								</complexContent>
							</complexType>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="processmodel">
		<annotation>
			<documentation>Represents the visualization model for process model</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ogl:VisualizationModelType">
					<sequence>
						<element ref="ogl:processconfig"/>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="processconfig">
		<annotation>
			<documentation>Represents the configuration for a process model</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ogl:VisualizationConfigType">
					<sequence>
						<element name="activityconfig" maxOccurs="unbounded">
							<complexType>
								<complexContent>
									<extension base="ogl:VisualizationSeriesContainerType">
										<sequence>
											<element ref="ogl:columnconfig" maxOccurs="2"/>
											<element ref="ogl:processconfig" minOccurs="0"/>
											<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0">
												<annotation>
													<documentation>Represents actions which are to be associated with a step via a menu</documentation>
												</annotation>
											</element>
											<element name="path" minOccurs="0" maxOccurs="unbounded">
												<complexType>
													<sequence>
														<element name="pathnode" type="ogl:CoordinatePointType" minOccurs="2" maxOccurs="unbounded"/>
													</sequence>
												</complexType>
											</element>
										</sequence>
										<attribute name="activityid" type="string" use="required"/>
										<attribute name="title" type="string" use="required"/>
										<attribute name="x" type="nonNegativeInteger" use="optional"/>
										<attribute name="y" type="nonNegativeInteger" use="optional"/>
										<attribute name="width" type="nonNegativeInteger" use="optional"/>
										<attribute name="height" type="nonNegativeInteger" use="optional"/>
										<attribute name="source" type="string" use="optional"/>
										<attribute name="root" type="ogl:BooleanOptions" use="optional"/>
										<attribute name="loopback" type="string" use="optional"/>
										<attribute name="skipto" type="string" use="optional"/>
									</extension>
								</complexContent>
							</complexType>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="pagesetselectormodel">
		<annotation>
			<documentation>Represents the visualization model for the page set selection component</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ogl:VisualizationModelType">
					<sequence>
						<element name="pagesetselectorconfig">
							<complexType>
								<complexContent>
									<extension base="ogl:VisualizationConfigType">
										<sequence>
											<element name="globalrowconfig" minOccurs="0">
												<complexType>
													<sequence>
														<element ref="ogl:columnconfig"/>
													</sequence>
												</complexType>
											</element>
											<element name="pagesetrowconfig" maxOccurs="unbounded">
												<complexType>
													<sequence>
														<element name="title" type="string"/>
														<element name="tooltip" type="string"/>
														<element ref="ogl:columnconfig" minOccurs="0" maxOccurs="2"/>
													</sequence>
													<attribute name="launchtype" type="ogl:PageSetLaunchType" use="optional"/>
													<attribute name="pagesetid" type="string" use="required"/>
												</complexType>
											</element>
										</sequence>
									</extension>
								</complexContent>
							</complexType>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<element name="compgallery">
		<annotation>
			<documentation>Represents the component gallery model for component/metric gallery</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="ogl:category" minOccurs="0" maxOccurs="unbounded"/>
			</sequence>
		</complexType>
	</element>
	<element name="category">
		<annotation>
			<documentation>Represents a individual category/folder in the component gallery</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="name" type="string"/>
				<element name="comp" type="ogl:ComponentDefinition" minOccurs="0" maxOccurs="unbounded"/>
				<element ref="ogl:category" minOccurs="0" maxOccurs="unbounded"/>
			</sequence>
			<attribute name="id" type="string" use="required"/>
			<attribute name="count" type="int" use="required"/>
			<attribute name="parentid" type="string" use="required"/>
		</complexType>
	</element>
	<element name="elementactionconfigs">
		<complexType>
			<sequence>
				<element name="element" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element name="actionconfig" type="ogl:ActionConfigType"/>
						</sequence>
						<attribute name="id" type="string" use="required"/>
						<attribute name="type" type="string" use="required"/>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
	<element name="updatepackets">
		<annotation>
			<documentation>Represents a set of updates streamed to the UI</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="updatepacket" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element ref="ogl:visualizationdata"/>
						</sequence>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
	<element name="busdaysettings">
		<complexType>
			<sequence>
				<element name="comptitle" type="string" minOccurs="0"/>
				<element name="seriesbusdaysetting" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element name="seriestitle" type="string" minOccurs="0"/>
							<element name="busdayoption" minOccurs="0" maxOccurs="unbounded">
								<complexType>
									<attribute name="id" type="string" use="required"/>
									<attribute name="name" type="string" use="required"/>
								</complexType>
							</element>
						</sequence>
						<attribute name="seriesid" type="string" use="required"/>
						<attribute name="selectedbusdayid" type="string" use="optional"/>
					</complexType>
				</element>
			</sequence>
			<attribute name="compid" type="string" use="required"/>
		</complexType>
	</element>
	<element name="drilldownmodel">
		<annotation>
			<documentation>Represents the visualization model for drilldown widget</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="ogl:textmodel" maxOccurs="unbounded"/>
				<element name="actionconfig" type="ogl:ActionConfigType" minOccurs="0"/>
			</sequence>
		</complexType>
	</element>
</schema>
