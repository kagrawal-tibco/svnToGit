<?xml version="1.0" encoding="UTF-8"?>
<views>
    
    <view command="search">
        <result type="nosearchpage" template="search/nosearchpage.vm"/>
        <result type="invalidsearchpage" template="search/invalidsearchpage.vm"/>
        <result type="drilldown" template="search/drilldown.vm"/>
        <result type="drilldownwithquerymgr" template="search/drilldownwithquerymanager.vm"/>
        <result type="error" template="search/error.vm"/>
    </view>
    
    <view command="querymgr">
        <result type="baserender" template="querymgr/baserender.vm"/>
    </view>
    
    <view command="getfields">
        <result type="success" template="querymgr/fieldsrenderer.vm"/>
        <result type="error" template="querymgr/fieldserrorrenderer.vm"/>
    </view>
    
    <view command="resetfieldcondition"/>
    
    <view command="viewfieldcondition">
        <result type="error" template="querymgr/fieldconditionerrorrenderer.vm"/>
        <result type="stringconditionview" template="querymgr/stringconditionrenderer.vm"/>
        <result type="booleanconditionview" template="querymgr/booleanconditionrenderer.vm"/>
        <result type="doubleconditionview" template="querymgr/numericconditionrenderer.vm"/>
        <result type="floatconditionview" template="querymgr/numericconditionrenderer.vm"/>
        <result type="intconditionview" template="querymgr/numericconditionrenderer.vm"/>
        <result type="longconditionview" template="querymgr/numericconditionrenderer.vm"/>
        <result type="shortconditionview" template="querymgr/numericconditionrenderer.vm"/>
        <result type="datetimeconditionview" template="querymgr/datetimeconditionrenderer.vm"/>    
    </view>    
    
    <view command="updatefieldcondition"/>
    
     <view command="executequery">
     	<result type="error" template="querymgr/queryexecuteerror.vm"/>
     </view>
     
    <view command="viewqueryexportoptions">
		<result type="success" template="querymgr/export/dialog.vm"/>
		<result type="error" template="querymgr/export/dialogerror.vm"/>
	</view>

	<view command="drilldown">
        <result type="baserender" template="drilldown/baserender.vm"/>
        <result type="baserendererror" template="drilldown/baseerrorrender.vm"/>
        <result type="emptybaserender" template="drilldown/baseemptyrender.vm"/>
        <result type="emptydrilldown" template="drilldown/drilldown-empty.vm"/>
		<result type="success" template="drilldown/drilldown.vm"/>
		<result type="inline" template="drilldown/drilldown-inline.vm"/>
		<result type="error" template="drilldown/error.vm"/>
		<result type="exportdialog" template="drilldown/error.vm"/>
	</view>
    
	<view command="showdrilldownexportdialog">
		<result type="success" template="drilldown/export/dialog.vm"/>
		<result type="error" template="drilldown/export/dialogerror.vm"/>
	</view>
    
	<view command="exportdrilldowncontent">
		<result type="error" template="drilldown/export/error.vm"/>
	</view>
	
	<view command="showrelatedconcepts">
		<result type="error" template="drilldown/relatedconcepts/error.vm"/>
		<result type="success" template="drilldown/relatedconcepts/success.vm"/>
	</view>
</views>