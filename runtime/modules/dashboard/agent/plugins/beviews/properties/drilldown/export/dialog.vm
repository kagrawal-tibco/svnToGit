<table border="0" cellspacing="0" cellpadding="0" width="100%" height="100%">
	<tr height="90px">
		<td id="contentpanel" align="center" class="exportpnl" colspan="3">
			<table border="0" cellspacing="0" cellpadding="0" width="90%">
				<form id="fileinputform" method="get" name="fileinputform" action="$exportURL" onsubmit="return false;">
				    <input type="hidden" name="mode" value="csv">
					<tr>
						<td>
							<table border="0" cellspacing="0" cellpadding="0" width="100%">
								<tr>
									<td colspan="2" align="left" class="inputlabel">Export Data As</td>
								</tr>
								<tr><td colspan="2">&nbsp</td></tr>
								<tr>
									<td>
										<input type="radio" name="exporttype" value="csv" checked onClick="chooseMode('csv')"/>&nbsp<font class="inputlabel">CSV</font></input>
									</td>
									<td>
										<input type="radio" name="exporttype" value="xml" onClick="chooseMode('xml')"/>&nbsp<font class="inputlabel">XML</font></input>
									</td>
								</tr>
								<tr><td colspan="2">&nbsp</td></tr>
							</table>
						</td>
					</tr>
				</form>
			</table>
		</td>
	</tr>
    <tr id="buttonPanel" class="exportbtnpnl">
        <td valign="middle">
            <a class="button" href="#" onclick="this.blur();ok_clicked();" style="width:100px"><span>Ok</span></a>
        </td>
        <td>    
            <a class="button" href="#" onclick="this.blur();transfervalue('');" style="width:100px"><span>Cancel</span></a>
        </td>
        <td width="100%">
            &nbsp;
        </td>
    </tr>        
</table>
