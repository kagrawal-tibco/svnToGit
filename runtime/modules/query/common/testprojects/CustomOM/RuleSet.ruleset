        <rule icon="" name="PersonEventGenRule" description="" guid="1192214991693-2043304569" bindings="" namespace="" isAFunction="false" isAConditionFunction="false" compilationStatus="-1">
            <lastModified>2007-10-12T12:13:49.807-07:00</lastModified>
            <hiddenProperties/>
            <priority>5</priority>
            <testInterval>-1</testInterval>
            <startTime>-1</startTime>
