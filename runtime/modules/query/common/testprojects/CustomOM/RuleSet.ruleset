            <maxRules>1</maxRules>
            <fwdChain>true</fwdChain>
            <bwdChain>false</bwdChain>
            <declarations>
                <declaration identifier="timerloopevent" entity="/TimerLoopEvent">
                    <domain>
                        <entries/>
                    </domain>
                </declaration>
            </declarations>
            <condition/>
            <action>double d = Math.random();
d = d * 10;
int i = d;

String[] lastNames = {"Smurf", "Flintstone", "Simpson"};
String[] firstNames = {"a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o", "p", "q", "r", "s", "t", "u", "v", "w", "x", "y", "z"};

int lastNamePos = i % lastNames@length;
int firstNamePos = i % firstNames@length;
String fn = firstNames[firstNamePos] + firstNames[firstNamePos] + firstNames[firstNamePos] + "_" + i;
int age = (d * 100) % 75;

SimpleEvent e = PersonEvent.PersonEvent("","", fn, lastNames[lastNamePos], age);
Event.assertEvent(e);
</action>
            <requeueVars/>
            <template/>
            <author/>
        </rule>
