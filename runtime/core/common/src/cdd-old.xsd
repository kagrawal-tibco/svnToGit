<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xml:lang="en"
	targetNamespace="http://tibco.com/businessevents/configuration/5.1"
	xmlns:cdd="http://tibco.com/businessevents/configuration/5.1"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
	attributeFormDefault="unqualified">

	<!-- - - - - - - - - Elements - - - - - - - - - -->

	<xsd:element name="accept-count" type="cdd:override-type" />
	<xsd:element name="address" type="cdd:override-type" />
	<xsd:element name="agent" type="cdd:agent-type" />
	<xsd:element name="agent-classes" type="cdd:agent-classes-type" />
	<xsd:element name="agents" type="cdd:agents-type" />
	<xsd:element name="alert-config" type="cdd:alert-config-type" />
