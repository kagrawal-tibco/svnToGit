	<xsd:element name="addon" ecore:name="AddOn"
		type="reg:addon" />
	
	<xsd:complexType ecore:name="AddOn" name="addon">
		<xsd:sequence>
			<xsd:element name="type" type="reg:AddOnType"></xsd:element>
			<xsd:element ecore:name="IndexResourceProviderClass"
				name="indexResourceProviderClass" type="xsd:string" />
			<xsd:element ecore:name="IndexCacheClass"
				name="indexCacheClass" type="xsd:string" />
			<xsd:element ecore:name="OntologyAdapterClass"
				name="ontologyAdapterClass" type="xsd:string" />
			<xsd:element ecore:name="AdapterFactoryClass"
				name="adapterFactoryClass" type="xsd:string" />
			<xsd:element ecore:name="TnsEntityHandlerClass"
				name="tnsEntityHandlerClass" type="xsd:string" />
			<xsd:element ecore:name="CddAgentClass"
				name="cddAgentClass" type="xsd:string" />
			<xsd:element ecore:name="SupportedEntityTypes"
				name="supportedTypes" type="reg:supportedElementTypes" />
			<xsd:element ecore:name="SupportedExtensions"
				name="supportedExtensions" type="reg:supportedExtensions" />
			<xsd:element ecore:name="FileExtensionMap"
				name="fileExtensionMap" type="reg:fileExtensionMapType"
				maxOccurs="unbounded" />
			<xsd:element ecore:name="TnsEntityExtensions"
				name="tnsEntityExtensions" type="reg:tnsEntityExtensions" />
			<xsd:element ecore:name="FunctionCatalogProvider"
				name="functionCatalogProvider" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType ecore:name="SupportedElementTypes"
		name="supportedElementTypes">
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="elementType"
				type="reg:ElementTypes" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType ecore:name="SupportedExtensions"
		name="supportedExtensions">
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="extension"
				type="reg:FileExtensionTypes" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType ecore:name="TnsEntityExtensions"
		name="tnsEntityExtensions">
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="extension"
				type="reg:FileExtensionTypes" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

    
  <xsd:complexType ecore:name="FileExtensionMapType" name="fileExtensionMapType">
    <xsd:sequence>
      <xsd:element ecore:name="Item" name="item" type="reg:FileExtensionMapItemType"
					minOccurs="0" maxOccurs="unbounded" />
    </xsd:sequence>
  </xsd:complexType>
    
  <xsd:complexType ecore:name="FileExtensionMapItemType" name="FileExtensionMapItemType">
    <xsd:sequence>
      <xsd:element ecore:name="Extension" name="key" type="reg:FileExtensionTypes"/>
      <xsd:element ecore:name="ElementType" name="value" type="reg:supportedElementTypes"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="AddOnType">
  	<xsd:restriction base="xsd:string">
  		<xsd:enumeration value="CORE"></xsd:enumeration>
  		<xsd:enumeration value="DATA_MODELLING"></xsd:enumeration>
  		<xsd:enumeration value="EVENTSTREAM_PROCESSING"></xsd:enumeration>
  		<xsd:enumeration value="DECISION_MANAGER"></xsd:enumeration>
  		<xsd:enumeration value="VIEWS"></xsd:enumeration>
  		<xsd:enumeration value="PROCESS"></xsd:enumeration>
  		<xsd:enumeration value="LIVEVIEW"></xsd:enumeration>
  	</xsd:restriction>
  </xsd:simpleType>


</xsd:schema>
