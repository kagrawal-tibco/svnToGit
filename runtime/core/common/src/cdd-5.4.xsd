	<xsd:complexType ecore:name="BackingStoreForDomainObjectConfig"
		name="backing-store-for-domain-object-type">
		<xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
					<body>return new java.util.Properties();</body>
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element form="qualified" minOccurs="0" name="properties"
				type="cdd:backing-store-for-properties-type">
				<xsd:annotation>
					<xsd:documentation>
						Backing store configuration for the domain
						object's properties.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="cdd:enabled">
				<xsd:annotation>
					<xsd:documentation>
						Enables storage in the backing store, if true.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="tableName" minOccurs="0"
				ref="cdd:table-name" />
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType ecore:name="BackingStoreForPropertiesConfig"
		name="backing-store-for-properties-type">
