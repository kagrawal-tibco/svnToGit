	<xsd:complexType ecore:name="BackingStoreConfig" name="backing-store-type">
	    <xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element ecore:name="cacheAside" minOccurs="0"
				ref="cdd:cache-aside">
				<xsd:annotation>
					<xsd:documentation>
						Enables cache-aside if true.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="cacheLoaderClass" minOccurs="0"
				ref="cdd:cache-loader-class">
				<xsd:annotation>
					<xsd:documentation>
						Coherence backing store implementation.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="dataStorePath" minOccurs="0"
				ref="cdd:data-store-path">
				<xsd:annotation>
					<xsd:documentation>
						Specifies the path for the data store.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>			
			<xsd:element ecore:name="enforcePools" minOccurs="0"
				ref="cdd:enforce-pools">
				<xsd:annotation>
					<xsd:documentation>
						Enables all the connection pools.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="persistenceOption" minOccurs="0"
				ref="cdd:persistence-option">
				<xsd:annotation>
					<xsd:documentation>
						Enables the backing store - None, Shared All, Shared Nothing
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="persistencePolicy" minOccurs="0"
				ref="cdd:persistence-policy">
				<xsd:annotation>
					<xsd:documentation>
						Persistence policy to be used (SYNC or ASYNC).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="parallelRecovery" minOccurs="0"
				ref="cdd:parallel-recovery">
				<xsd:annotation>
					<xsd:documentation>
						Parallel Recovery.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="primaryConnection" minOccurs="0"
				ref="cdd:primary-connection">
				<xsd:annotation>
					<xsd:documentation>
						Primary connection to the database.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="secondaryConnection" minOccurs="0"
				ref="cdd:secondary-connection">
				<xsd:annotation>
					<xsd:documentation>
						Secondary connection to the database.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="cdd:strategy">
				<xsd:annotation>
					<xsd:documentation>
						Whether to use jdbc or proprietary mode.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="cdd:type">
				<xsd:annotation>
					<xsd:documentation>
						Database type, e.g. oracle or sqlserver.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
