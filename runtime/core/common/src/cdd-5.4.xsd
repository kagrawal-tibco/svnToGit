	<xsd:complexType ecore:name="HttpConfig" name="http-type">
		<xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
					<body>return new java.util.Properties();</body>
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element ecore:name="acceptCount" minOccurs="0"
				ref="cdd:accept-count">
				<xsd:annotation>
					<xsd:documentation>
						Maximum queue length for incoming connection
						requests. Any requests received when the queue
						is full will be
						refused.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="connectionLinger" minOccurs="0"
				ref="cdd:connection-linger">
				<xsd:annotation>
					<xsd:documentation>
						Number of milliseconds during which the sockets
						used by the HTTP server will linger when they
						are closed.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="connectionTimeout" minOccurs="0"
				ref="cdd:connection-timeout">
				<xsd:annotation>
					<xsd:documentation>
						Number of milliseconds the HTTP server will
						wait, after accepting a connection, for the
						request URI line to be
						presented.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="documentPage" minOccurs="0"
				ref="cdd:document-page">
				<xsd:annotation>
					<xsd:documentation>
						Default static html file name.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="documentRoot" minOccurs="0"
				ref="cdd:document-root">
				<xsd:annotation>
					<xsd:documentation>
						Location of static documents.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="maxProcessors" minOccurs="0"
				ref="cdd:max-processors">
				<xsd:annotation>
					<xsd:documentation>
						Maximum number of simultaneous requests that can
						be handled by the HTTP server.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="socketOutputBufferSize"
				minOccurs="0" ref="cdd:socket-output-buffer-size">
				<xsd:annotation>
					<xsd:documentation>
						The size (in bytes) of the socket output buffer.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="cdd:ssl" />
			<xsd:element ecore:name="staleConnectionCheck" minOccurs="0"
				ref="cdd:stale-connection-check">
				<xsd:annotation>
					<xsd:documentation>
						Whether stale connection check is to be used.
						Disabling stale connection check may result in
						slight performance
						improvement at the risk of
						getting an I/O error when executing a
						request.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ecore:name="tcpNoDelay" minOccurs="0"
				ref="cdd:tcp-no-delay">
				<xsd:annotation>
					<xsd:documentation>
						Value of TCP_NO_DELAY for the server socket. Set
						to true to improve performance under most
						circumstances.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType ecore:name="IndexesConfig" name="indexes-type">
