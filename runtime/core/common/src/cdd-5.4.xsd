	<xsd:complexType ecore:name="RevisionConfig" name="revision-type">
		<xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
					<body>return new java.util.Properties();</body>
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element ref="cdd:version" />
			<xsd:element minOccurs="0" ref="cdd:author" />
			<xsd:element minOccurs="0" ref="cdd:date" />
			<xsd:element minOccurs="0" ref="cdd:comment" />
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType ecore:name="RuleConfigConfig" name="rule-config-type">
		<xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
					<body>return new java.util.Properties();</body>
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ecore:name="clusterMember" maxOccurs="unbounded"
				ref="cdd:cluster-member" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType ecore:name="RulesetsConfig" name="rulesets-type">
		<xsd:complexContent>
			<xsd:extension base="cdd:artifact-type">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0"
						ref="cdd:rules" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType ecore:name="RulesConfig" name="rules-type">
		<xsd:complexContent>
			<xsd:extension base="cdd:uris-and-refs-type">
				<xsd:sequence>
					<xsd:choice ecore:featureMap="group" maxOccurs="unbounded">
						<xsd:element
							ecore:upperBound="-1"
							ecore:reference="cdd:rules-type"
							minOccurs="0" name="ref" type="xsd:IDREF"/>
						<xsd:element ecore:upperBound="-1" minOccurs="0" ref="cdd:uri" />
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType ecore:name="RuleConfig" name="rule-type">
		<xsd:complexContent>
			<xsd:extension base="cdd:artifact-type">
				<xsd:attribute default="true" name="enabled" type="xsd:boolean" />
				<xsd:attribute name="uri" type="cdd:ontology-uri-type"
					use="required" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType abstract="true" ecore:name="SecurityConfig" name="security-config-type">
	    <xsd:annotation>
	      <xsd:appinfo ecore:key="operations" source="http://www.eclipse.org/emf/2002/Ecore">
	        <operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
	          <body>return new java.util.Properties();</body>
	        </operation>
	      </xsd:appinfo>
	    </xsd:annotation>
	</xsd:complexType>
	
	<xsd:complexType ecore:name="SecurityController" name="security-controller-type">
	  <xsd:sequence>
	    <xsd:element ecore:name="policyFile" minOccurs="0" ref="cdd:policy-file"/>
	    <xsd:element ecore:name="identityPassword" minOccurs="0" ref="cdd:identity-password"/>
	  </xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType ecore:name="SecurityOverrideConfig" name="security-override-type" mixed="true">
	  <xsd:complexContent mixed="true">
	    <xsd:extension base="cdd:override-type">
	      <xsd:attribute ecore:unsettable="false" name="override" type="xsd:boolean"/>
	    </xsd:extension>
	  </xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType ecore:name="SecurityRequester" name="security-requester-type">
	  <xsd:sequence>
	    <xsd:element ecore:name="tokenFile" minOccurs="0" ref="cdd:token-file"/>
	    <xsd:element ecore:name="identityPassword" minOccurs="0" ref="cdd:identity-password"/>
	    <xsd:element ecore:name="certificateKeyFile" minOccurs="0" ref="cdd:certificate-key-file"/>
	    <xsd:element ecore:name="domainName" minOccurs="0" ref="cdd:domain-name"/>
	    <xsd:element ecore:name="userName" minOccurs="0" ref="cdd:user-name"/>
	    <xsd:element ecore:name="userPassword" minOccurs="0" ref="cdd:user-password"/>
	  </xsd:sequence>
	</xsd:complexType>
		
	<xsd:complexType ecore:name="SetPropertyConfig" name="set-property-type">
		<xsd:all>
			<xsd:element ecore:name="childClusterMember" minOccurs="0"
				ref="cdd:child-cluster-member" />
			<xsd:element minOccurs="0" ref="cdd:notification" />
		</xsd:all>
		<xsd:attribute name="name" type="xsd:string" />
		<xsd:attribute ecore:name="setPropertyName" name="set-property-name"
			type="xsd:string" />
		<xsd:attribute name="value" type="xsd:string" />
	</xsd:complexType>
	<xsd:complexType ecore:name="SharedQueueConfig" name="shared-queue-type">
		<xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element minOccurs="0" ref="cdd:size" />
			<xsd:element minOccurs="0" ref="cdd:workers" />
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType ecore:name="SslConfig" name="ssl-type">
		<xsd:all>
			<xsd:element ref="cdd:protocols" />
			<xsd:element ref="cdd:ciphers" />
			<xsd:element ecore:name="keyManagerAlgorithm" ref="cdd:key-manager-algorithm" />
			<xsd:element ecore:name="trustManagerAlgorithm" ref="cdd:trust-manager-algorithm" />
		</xsd:all>
	</xsd:complexType>

	<xsd:complexType ecore:name="SystemPropertyConfig" mixed="true"
		name="system-property-type">
		<xsd:attribute ecore:name="systemProperty" name="system-property"
			type="xsd:string" />
	</xsd:complexType>
	<xsd:complexType ecore:name="TerminalConfig" name="terminal-type">
        <xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element minOccurs="0" ref="cdd:enabled" />
			<xsd:element ecore:name="sysErrRedirect" minOccurs="0"
				ref="cdd:sys-err-redirect" />
			<xsd:element ecore:name="sysOutRedirect" minOccurs="0"
				ref="cdd:sys-out-redirect" />
			<xsd:element minOccurs="0" ref="cdd:encoding" />
		</xsd:all>
	</xsd:complexType>
	<xsd:simpleType ecore:name="ThreadingModelConfig" name="threading-model-type">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration ecore:name="sharedQueue" value="shared-queue" />
			<xsd:enumeration value="workers" />
			<xsd:enumeration value="caller" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType abstract="true" ecore:name="UrisAndRefsConfig" name="uris-and-refs-type">
        <xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
                <operation name="getAllUris" type="ecore:EEList{ecore:EString}"/>
                <operation name="getRef" type="ecore:EEList{? extends cdd:uris-and-refs-type}"/>
                <operation name="getUri" type="ecore:EEList{ecore:EString}"/>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cdd:artifact-type"/>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType ecore:name="UrisConfig" name="uris-type">
		<xsd:complexContent>
			<xsd:extension base="cdd:artifact-type">
				<xsd:sequence>
					<xsd:choice ecore:featureMap="group" maxOccurs="unbounded">
						<xsd:element ecore:upperBound="-1" minOccurs="0" ref="cdd:uri" />
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType ecore:name="FieldEncryptionConfig" name="encryption-type">
    	<xsd:annotation>
      		<xsd:appinfo ecore:key="operations" source="http://www.eclipse.org/emf/2002/Ecore">
        		<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
          			<body>return new java.util.Properties();</body>
        		</operation>
      		</xsd:appinfo>
    	</xsd:annotation>
    	<xsd:sequence>
      		<xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="property" type="cdd:override-type"/>
    	</xsd:sequence>
  	</xsd:complexType>
</xsd:schema>
