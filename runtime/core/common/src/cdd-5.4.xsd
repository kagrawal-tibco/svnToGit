			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
					<body>return new java.util.Properties();</body>
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:all>
			<xsd:element ref="cdd:name" />
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType ecore:name="QueryAgentClassConfig" name="query-agent-class-type">
		<xsd:complexContent>
			<xsd:extension base="cdd:agent-class-type">
				<xsd:all>
					<xsd:element minOccurs="0" ref="cdd:destinations" />
					<xsd:element ref="cdd:startup" />
					<xsd:element minOccurs="0" ref="cdd:shutdown" />
					<xsd:element minOccurs="0" ref="cdd:load" />
					<xsd:element ecore:name="localCache" minOccurs="0"
						ref="cdd:local-cache" />
					<xsd:element ecore:name="propertyGroup" minOccurs="0"
						ref="cdd:property-group" />
					<xsd:element ecore:name="sharedQueue" minOccurs="0"
						ref="cdd:shared-queue" />
				</xsd:all>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
