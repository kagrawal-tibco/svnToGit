	<xsd:complexType ecore:name="AgentClassesConfig" name="agent-classes-type">
		<xsd:annotation>
			<xsd:appinfo ecore:key="operations"
				source="http://www.eclipse.org/emf/2002/Ecore">
				<operation name="toProperties" type="ecore:EMap{ecore:EJavaObject, ecore:EJavaObject}">
					<body>return new java.util.Properties();</body>
				</operation>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice ecore:featureMap="group" maxOccurs="unbounded">
				<xsd:element ecore:name="cacheAgentConfig"
					ecore:upperBound="-1" minOccurs="0" ref="cdd:cache-agent-class" />
				<xsd:element ecore:name="dashboardAgentConfig"
					ecore:upperBound="-1" minOccurs="0" ref="cdd:dashboard-agent-class" />
				<xsd:element ecore:name="inferenceAgentConfig"
					ecore:upperBound="-1" minOccurs="0" ref="cdd:inference-agent-class" />
				<xsd:element ecore:name="queryAgentConfig"
					ecore:upperBound="-1" minOccurs="0" ref="cdd:query-agent-class" />
				<xsd:element ecore:name="mmAgentConfig" ecore:upperBound="-1"
					minOccurs="0" ref="cdd:mm-agent-class" />
				<xsd:element ecore:name="processAgentConfig"
					ecore:upperBound="-1" minOccurs="0" ref="cdd:process-agent-class" />
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	
