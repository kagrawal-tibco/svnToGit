			<xsd:extension base="cdd:artifact-type">
				<xsd:sequence>
					<xsd:choice ecore:featureMap="group" maxOccurs="unbounded">
						<xsd:element ecore:upperBound="-1" form="qualified"
							minOccurs="0" name="property" type="cdd:backing-store-for-property-type" />
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
