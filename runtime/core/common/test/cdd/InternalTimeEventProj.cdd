<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cluster xmlns="http://tibco.com/businessevents/configuration/5.1">
    <revision>
        <version>4</version>
        <author>TIBCO Software Inc.</author>
        <date>2010/10/11 11:48:36</date>
        <comment>CDD for InternalTimeEvent example</comment>
    </revision>
    <name>InternalTimeEvent</name>
    <message-encoding>UTF-8</message-encoding>
    <object-management>
        <memory-manager/>
        <db-concepts>
            <db-uris/>
            <check-interval>60</check-interval>
            <inactivity-timeout>0</inactivity-timeout>
            <initial-size>5</initial-size>
            <max-size>5</max-size>
            <min-size>5</min-size>
            <property-check-interval>900</property-check-interval>
            <retry-count>-1</retry-count>
            <wait-timeout>1</wait-timeout>
        </db-concepts>
    </object-management>
    <rulesets/>
    <function-groups/>
    <destination-groups/>
    <log-configs>
        <log-config id="logConfig">
            <enabled>true</enabled>
            <roles>*:info</roles>
            <files>
                <enabled>true</enabled>
                <dir>logs</dir>
                <name>InternalTimeEvent.log</name>
                <max-number>10</max-number>
                <max-size>10000000</max-size>
                <append>true</append>
            </files>
            <terminal>
                <enabled/>
                <sys-err-redirect/>
                <sys-out-redirect/>
            </terminal>
            <line-layout>
                <enabled>false</enabled>
                <class/>
                <arg/>
            </line-layout>
        </log-config>
    </log-configs>
    <agent-classes>
        <inference-agent-class id="InternalTimeEvent">
            <rules>
                <uri>/</uri>
            </rules>
            <destinations/>
            <startup>
                <uri>/RuleFunctions/Startup</uri>
            </startup>
            <shutdown/>
            <local-cache>
                <eviction>
                    <max-size/>
                    <max-time/>
                </eviction>
            </local-cache>
            <concurrent-rtc>false</concurrent-rtc>
            <businessworks>
                <uri/>
            </businessworks>
            <property-group/>
        </inference-agent-class>
    </agent-classes>
    <processing-units>
        <processing-unit id="default">
            <agents>
                <agent>
                    <ref>InternalTimeEvent</ref>
                    <key/>
                    <priority/>
                </agent>
            </agents>
            <logs>logConfig</logs>
            <hot-deploy>false</hot-deploy>
            <db-concepts>false</db-concepts>
            <http>
                <connection-timeout>60000</connection-timeout>
                <accept-count>-1</accept-count>
                <connection-linger>-1</connection-linger>
                <socket-output-buffer-size>9000</socket-output-buffer-size>
                <max-processors>-1</max-processors>
                <tcp-no-delay>true</tcp-no-delay>
                <document-root/>
                <document-page/>
                <stale-connection-check>false</stale-connection-check>
                <ssl>
                    <key-manager-algorithm/>
                    <trust-manager-algorithm/>
                    <protocols/>
                    <ciphers/>
                </ssl>
            </http>
            <property-group/>
        </processing-unit>
    </processing-units>
    <property-group/>
</cluster>
