<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cdd:cluster xmlns:cdd="http://tibco.com/businessevents/configuration/5.1">
    <cdd:revision>
        <cdd:version>5</cdd:version>
        <cdd:author>mgehlot</cdd:author>
        <cdd:date>2010/09/07 14:04:06</cdd:date>
        <cdd:comment/>
    </cdd:revision>
    <cdd:name>PCRF</cdd:name>
    <cdd:message-encoding>UTF-8</cdd:message-encoding>
    <cdd:object-management>
        <cdd:memory-manager/>
        <cdd:db-concepts>
            <cdd:db-uris/>
            <cdd:check-interval>60</cdd:check-interval>
            <cdd:inactivity-timeout>0</cdd:inactivity-timeout>
            <cdd:initial-size>5</cdd:initial-size>
            <cdd:max-size>5</cdd:max-size>
            <cdd:min-size>5</cdd:min-size>
            <cdd:property-check-interval>900</cdd:property-check-interval>
            <cdd:retry-count>-1</cdd:retry-count>
            <cdd:wait-timeout>1</cdd:wait-timeout>
        </cdd:db-concepts>
    </cdd:object-management>
    <cdd:rulesets>
        <cdd:rules id="all-rules">
            <cdd:uri>/</cdd:uri>
        </cdd:rules>
    </cdd:rulesets>
    <cdd:function-groups>
        <cdd:functions id="all-functions">
            <cdd:uri>/</cdd:uri>
        </cdd:functions>
    </cdd:function-groups>
    <cdd:destination-groups/>
    <cdd:log-configs>
        <cdd:log-config id="logConfig">
            <cdd:enabled>true</cdd:enabled>
            <cdd:roles>*:info</cdd:roles>
            <cdd:files>
                <cdd:enabled>true</cdd:enabled>
                <cdd:dir>logs</cdd:dir>
                <cdd:name/>
                <cdd:max-number>10</cdd:max-number>
                <cdd:max-size>5000000</cdd:max-size>
                <cdd:append>true</cdd:append>
            </cdd:files>
            <cdd:terminal>
                <cdd:enabled/>
                <cdd:sys-err-redirect/>
                <cdd:sys-out-redirect/>
            </cdd:terminal>
            <cdd:line-layout>
                <cdd:enabled>false</cdd:enabled>
                <cdd:class/>
                <cdd:arg/>
            </cdd:line-layout>
        </cdd:log-config>
    </cdd:log-configs>
    <cdd:agent-classes>
        <cdd:inference-agent-class id="inference-class">
            <cdd:rules>
                <cdd:ref>all-rules</cdd:ref>
            </cdd:rules>
            <cdd:destinations>
                <cdd:destination id="CCRDestination_5EAF6FDC">
                    <cdd:pre-processor/>
                    <cdd:queue-size/>
                    <cdd:thread-count/>
                    <cdd:threading-model>shared-queue</cdd:threading-model>
                    <cdd:uri>/Channels/RVChannel2/CCRDestination</cdd:uri>
                </cdd:destination>
            </cdd:destinations>
            <cdd:startup>
                <cdd:uri>/RuleFunctions/StartupRF</cdd:uri>
            </cdd:startup>
            <cdd:shutdown/>
            <cdd:local-cache>
                <cdd:eviction>
                    <cdd:max-size/>
                    <cdd:max-time/>
                </cdd:eviction>
            </cdd:local-cache>
            <cdd:concurrent-rtc>false</cdd:concurrent-rtc>
            <cdd:businessworks>
                <cdd:uri/>
            </cdd:businessworks>
            <cdd:property-group/>
        </cdd:inference-agent-class>
    </cdd:agent-classes>
    <cdd:processing-units>
        <cdd:processing-unit id="default">
            <cdd:agents>
                <cdd:agent>
                    <cdd:ref>inference-class</cdd:ref>
                    <cdd:key/>
                    <cdd:priority/>
                </cdd:agent>
            </cdd:agents>
            <cdd:logs>logConfig</cdd:logs>
            <cdd:hot-deploy>false</cdd:hot-deploy>
            <cdd:db-concepts>false</cdd:db-concepts>
            <cdd:http>
                <cdd:connection-timeout>60000</cdd:connection-timeout>
                <cdd:accept-count>-1</cdd:accept-count>
                <cdd:connection-linger>-1</cdd:connection-linger>
                <cdd:socket-output-buffer-size>9000</cdd:socket-output-buffer-size>
                <cdd:max-processors>-1</cdd:max-processors>
                <cdd:tcp-no-delay>true</cdd:tcp-no-delay>
                <cdd:document-root/>
                <cdd:document-page/>
                <cdd:stale-connection-check>false</cdd:stale-connection-check>
                <cdd:ssl>
                    <cdd:key-manager-algorithm/>
                    <cdd:trust-manager-algorithm/>
                    <cdd:protocols/>
                    <cdd:ciphers/>
                </cdd:ssl>
            </cdd:http>
            <cdd:property-group/>
        </cdd:processing-unit>
    </cdd:processing-units>
    <cdd:property-group/>
</cdd:cluster>
